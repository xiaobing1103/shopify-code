{%- style -%}
  {{ heading_font | font_face: font_display: 'swap' }}
  {{ text_font | font_face: font_display: 'swap' }}
  {{ column_heading_font | font_face: font_display: 'swap' }}
  {{ row_heading_font | font_face: font_display: 'swap' }}
  {{ value_font | font_face: font_display: 'swap' }}
  
  .section-{{ section.id }} {
    border-top: solid {{ border_color }} {{ border_thickness }}px;
    border-bottom: solid {{ border_color }} {{ border_thickness }}px;
    margin-top: {{ margin_top | times: 0.75 | round: 0 }}px;
    margin-bottom: {{ margin_bottom | times: 0.75 | round: 0 }}px;
    margin-left: {{ margin_horizontal_mobile }}rem;
    margin-right: {{ margin_horizontal_mobile }}rem;
    border-radius: {{ section_radius | times: 0.6 | round: 0 }}px;
    overflow: hidden;
  }
  
  .section-{{ section.id }}-settings {
    margin: 0 auto;
    padding-top: {{ padding_top | times: 0.75 | round: 0 }}px;
    padding-bottom: {{ padding_bottom | times: 0.75 | round: 0 }}px;
    padding-left: {{ padding_horizontal_mobile }}rem;
    padding-right: {{ padding_horizontal_mobile }}rem;
  }  

  
  .comparison-heading-{{ section.id }} {
    text-align: {{ content_align_mobile }};
  }

  .comparison-heading-{{ section.id }} * {
    margin: 0;
    font-size: {{ heading_size_mobile }}px;
    color: {{ heading_color }};
    line-height: {{ heading_height }}%;
    text-transform: unset;
    text-decoration: none;
  }

  .comparison-text-{{ section.id }} {
    margin-top: {{ text_mt_mobile }}px;
    text-align: {{ content_align_mobile }};
  }

  .comparison-text-{{ section.id }} * {
    margin: 0;
    font-size: {{ text_size_mobile }}px;
    color: {{ text_color }};
    line-height: {{ text_height }}%;
    text-transform: unset;
    text-decoration: none;
  }

  .comparison-grid-{{ section.id }} {
    display: flex;
    gap: 25px;
  }

  .comparison-table-wrapper-{{ section.id }} {
    padding-top: {{ heading_image_size_mobile | divided_by: 2 }}px;
    overflow-x: scroll;
    scrollbar-width: none;
    margin-left: -{{ padding_horizontal_mobile }}rem;
    margin-right: -{{ padding_horizontal_mobile }}rem;
  }

  .comparison-table-inner-{{ section.id }} {
    min-width: 724px;
    padding-left: {{ padding_horizontal_mobile }}rem;
    padding-right: {{ padding_horizontal_mobile }}rem;
  }

  .comparison-table-wrapper-{{ section.id }}::-webkit-scrollbar {
    display: none;
  }

  .comparison-table-{{ section.id }} {
    padding-bottom: 25px;
    padding-right: 25px;
    margin-top: {{ table_mt_mobile }}px;
  }

  .comparison-item-{{ section.id }} {
    position: relative;
    display: flex !important;
    flex-direction: column;
    align-items: center;
    justify-content: center;
  }

  .comparison-item-bg-image-{{ section.id }} {
    position: absolute;
    z-index: 1;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    border-top-left-radius: {{ table_radius }}px;
    border-top-right-radius: {{ table_radius }}px;
    overflow: hidden;
  }
  
  .comparison-item-bg-image-{{ section.id }} img,
  .comparison-item-bg-image-{{ section.id }} svg {
    display: block;
    width: 100%;
    height: 100%;
    object-fit: cover;
  }

  .comparison-{{ section.id }} .comparison-item-{{ section.id }} .comparison-item-bg-image-{{ section.id }} svg defs linearGradient stop {
    stop-color: {{ table_bg_image_color }} !important;
  }

  .comparison-{{ section.id }} .comparison-item-{{ section.id }}.active .comparison-item-bg-image-{{ section.id }} svg defs linearGradient stop {
    stop-color: {{ table_bg_image_active_color }} !important;
  }
  
  .comparison-item-{{ section.id }}:first-child {
    flex-direction: row;
    justify-content: start;
    gap: 20px;
    padding: {{ column_heading_padding_vertical_mobile }} {{ }}
  }

  .comparison-grid-rows-{{ section.id }} .comparison-item-{{ section.id }}:first-child:after {
    content: '';
    position: absolute;
    bottom: 0;
    left: 0%;
    width: 70%;
    height: {{ row_border_thickness }}px;
    background-color: {{ row_heading_border_color }};
    mask-image: linear-gradient(to right, rgba(0, 0, 0, 0) 0%, rgb(0, 0, 0) 0%, rgb(0, 0, 0) 57%, rgba(0, 0, 0, 0) 100%);
  }

  .comparison-grid-header-{{ section.id }} .comparison-item-{{ section.id }} {
    padding: {{ column_heading_padding_vertical_mobile }}px {{ column_heading_padding_horizontal_mobile }}px;  
  }

  .comparison-grid-header-{{ section.id }} .comparison-item-{{ section.id }}:not(:first-child) {
    border-top: {{ table_column_border_thickness }}px solid {{ table_column_border_color }};
    border-left: {{ table_column_border_thickness }}px solid {{ table_column_border_color }};
    border-right: {{ table_column_border_thickness }}px solid {{ table_column_border_color }};
  }

  .comparison-grid-header-{{ section.id }} .comparison-item-{{ section.id }}:not(:first-child).active {
    border-top: {{ table_column_border_thickness }}px solid {{ table_column_active_border_color }};
    border-left: {{ table_column_border_thickness }}px solid {{ table_column_active_border_color }};
    border-right: {{ table_column_border_thickness }}px solid {{ table_column_active_border_color }};
  }

  .comparison-grid-rows-{{ section.id }} .comparison-item-{{ section.id }}:not(:first-child) {
    border-left: {{ table_column_border_thickness }}px solid {{ table_column_border_color }};
    border-right: {{ table_column_border_thickness }}px solid {{ table_column_border_color }};
  }

  .comparison-grid-rows-{{ section.id }} .comparison-item-{{ section.id }}:not(:first-child).active {  
    border-left: {{ table_column_border_thickness }}px solid {{ table_column_active_border_color }};
    border-right: {{ table_column_border_thickness }}px solid {{ table_column_active_border_color }};
  }

  .comparison-grid-rows-{{ section.id }}:last-child .comparison-item-{{ section.id }}:not(:first-child) {
    border-left: {{ table_column_border_thickness }}px solid {{ table_column_border_color }};
    border-right: {{ table_column_border_thickness }}px solid {{ table_column_border_color }};
    border-bottom: {{ table_column_border_thickness }}px solid {{ table_column_border_color }};
  }

  .comparison-grid-rows-{{ section.id }}:last-child .comparison-item-{{ section.id }}:not(:first-child).active {  
    border-left: {{ table_column_border_thickness }}px solid {{ table_column_active_border_color }};
    border-right: {{ table_column_border_thickness }}px solid {{ table_column_active_border_color }};
    border-bottom: {{ table_column_border_thickness }}px solid {{ table_column_active_border_color }};
  }

  .comparison-grid-header-{{ section.id }} .comparison-item-{{ section.id }}:not(:first-child),
  .comparison-grid-rows-{{ section.id }} .comparison-item-{{ section.id }}:not(:first-child) {
    background-color: {{ table_column_bg_color }};
  }

  .comparison-grid-header-{{ section.id }} .comparison-item-{{ section.id }}.active {
    background-color: {{ table_column_active_bg_color }};    
  }

  .comparison-grid-rows-{{ section.id }}:not(:last-child) .comparison-item-{{ section.id }}:not(:first-child):after {
    content: '';
    position: absolute;
    bottom: 0;
    left: 50%;
    transform: translateX(-50%);
    width: 60%;
    height: {{ row_border_thickness }}px;
    background-color: {{ table_column_border_color }};
  }

  .comparison-grid-rows-{{ section.id }}:not(:last-child) .comparison-item-{{ section.id }}.active:after {
    background-color: {{ table_column_active_border_color }};
  }

  .comparison-grid-header-{{ section.id }} .comparison-item-{{ section.id }}.active {
    border: 0px;
  }

  .comparison-grid-header-{{ section.id }} .comparison-column-heading-{{ section.id }} {
    margin: 16px 0 0 0;
    width: 100%;
    position: relative;
    z-index: 2;
    font-size: {{ column_heading_size_mobile }}px;
    color: {{ column_heading_color }};
    line-height: {{ column_heading_height }}%;
    text-transform: unset;
    text-align: {{ column_heading_align_mobile }};
    text-decoration: none;
    font-weight: 700;
  }

  .comparison-grid-header-{{ section.id }} .comparison-item-{{ section.id }}.active h3,
  .comparison-grid-header-{{ section.id }} .comparison-item-{{ section.id }}.active a {
    color: {{ column_heading_active_color }};
  }

  .comparison-grid-rows-{{ section.id }} .comparison-item-{{ section.id }} {
    padding: {{ row_padding_vertical_mobile }}px {{ row_padding_horizontal_mobile }}px;
  }

  .comparison-grid-header-{{ section.id }} + .comparison-grid-rows-{{ section.id }} .comparison-item-{{ section.id }}:first-child {
    border-top-left-radius: {{ table_radius }}px;
  }

  .comparison-grid-header-{{ section.id }} .comparison-item-{{ section.id }} {
    border-top-left-radius: {{ table_radius }}px;
    border-top-right-radius: {{ table_radius }}px;
  }

  .comparison-grid-rows-{{ section.id }}:last-child .comparison-item-{{ section.id }} {
    border-bottom-left-radius: {{ table_radius }}px;
    border-bottom-right-radius: {{ table_radius }}px;
  }

  .comparison-grid-header-{{ section.id }} .comparison-item-{{ section.id }}:nth-child(3) {
    border-top-left-radius: {{ table_radius }}px;
  }

  .comparison-grid-rows-{{ section.id }} .comparison-item-{{ section.id }}.active {
    background-color: {{ table_column_active_bg_color }};
  }

  .comparison-grid-header-{{ section.id }} .comparison-item-{{ section.id }}:first-child {
    border: 0px;
  }

  .comparison-grid-rows-{{ section.id }} .comparison-item-{{ section.id }}:first-child h3 {
    display: block;
    margin: 0;
    font-size: {{ row_heading_size_mobile }}px;
    color: {{ row_heading_color }};
    line-height: {{ row_heading_height }}%;
    text-transform: unset;
    text-align: left;
    font-weight: 700;
  }

  .comparison-item-heading-image-{{ section.id }} {
    display: block;
    position: relative;
    z-index: 2;
    margin-top: calc(-{{ heading_image_size_mobile | divided_by: 2 }}px - {{ column_heading_padding_vertical_mobile }}px);
    border: {{ heading_image_border_thickness }}px solid {{ heading_image_border_color }};
    border-radius: {{ heading_image_radius }}px;
    width: 100%;
    max-width: {{ heading_image_size_mobile }}px;
    overflow: hidden;
    aspect-ratio: 1/1;
  }

  .comparison-item-heading-image-{{ section.id }} img,
  .comparison-item-heading-image-{{ section.id }} svg {
    display: block;
    width: 100%;
    height: 100%;
    object-fit: cover;
  }

  .comparison-item-heading-image-{{ section.id }} svg {
    background-color: #AFAFAF;
  }

  .comparison-value-{{ section.id }} {
    margin: 0;
    font-size: {{ value_size_mobile }}px;
    color: {{ value_color }};
    line-height: {{ value_height }}%;
    word-break: break-word;
    text-transform: unset;
    text-align: center;
  }

  .comparison-item-{{ section.id }}.active .comparison-value-{{ section.id }} {
    color: {{ value_active_color }};
  }

  .comparison-value-icon-{{ section.id }} {
    width: {{ value_icon_size_mobile }}px;
    height: {{ value_icon_size_mobile }}px;    
  }

  .comparison-value-icon-{{ section.id }} img,
  .comparison-value-icon-{{ section.id }} svg {
    display: block;
    object-fit: cover;
    width: 100%;
    height: 100%;
  }

  .comparison-value-icon-{{ section.id }} svg path {
    stroke: transparent;
  }

  .comparison-value-icon-{{ section.id }} svg path {
    fill: {{ value_icon_color }}
  }

  .comparison-item-{{ section.id }}.active .comparison-value-icon-{{ section.id }} svg path {
    fill: {{ value_icon_active_color }}
  }
  
   .comparison-grid-header-{{ section.id }} .comparison-item-{{ section.id }} {
      width: 220px;
   }

   .comparison-grid-rows-{{ section.id }} .comparison-item-{{ section.id }} {
     width: 220px;
   }
  
  @media(min-width: 1024px) {

    .section-{{ section.id }} {
      margin-top: {{ margin_top }}px;
      margin-bottom: {{ margin_bottom }}px;
      margin-left: {{ margin_horizontal }}rem;
      margin-right: {{ margin_horizontal }}rem;
      border-radius: {{ section_radius }}px;
    }
    
    .section-{{ section.id }}-settings {
      padding: 0 5rem;
      padding-top: {{ padding_top }}px;
      padding-bottom: {{ padding_bottom }}px;
      padding-left: {{ padding_horizontal }}rem;
      padding-right: {{ padding_horizontal }}rem;
    }  
    
    .comparison-heading-{{ section.id }} {
      text-align: {{ content_align }};
    }
  
    .comparison-heading-{{ section.id }} * {
      font-size: {{ heading_size }}px;
    }

    .comparison-text-{{ section.id }} {
      margin-top: {{ text_mt }}px;
      text-align: {{ content_align }};
    }
  
    .comparison-text-{{ section.id }} * {
      font-size: {{ text_size }}px;
    }

    .comparison-grid-{{ section.id }} {
      display: flex;
      gap: 25px;
      justify-content: center;
    }

    .comparison-table-wrapper-{{ section.id }} {
      padding-top: {{ heading_image_size | divided_by: 2 }}px;
      margin-left: 0rem;
      margin-right: 0rem;
    }

    .comparison-table-inner-{{ section.id }} {
      padding-left: 0rem;
      padding-right: 0rem;
    }

    .comparison-table-{{ section.id }} {
      margin-top: {{ table_mt }}px;
    }

    .comparison-item-heading-image-{{ section.id }} {
      margin-top: calc(-{{ heading_image_size | divided_by: 2 }}px - {{ column_heading_padding_vertical }}px);
      max-width: {{ heading_image_size }}px;
    }

    .comparison-grid-header-{{ section.id }} .comparison-item-{{ section.id }} {
      padding: {{ column_heading_padding_vertical }}px {{ column_heading_padding_horizontal }}px;
    }

    .comparison-grid-header-{{ section.id }} .comparison-column-heading-{{ section.id }} {
      text-align: {{ column_heading_align }};
      font-size: {{ column_heading_size }}px;
    }

    .comparison-grid-rows-{{ section.id }} .comparison-item-{{ section.id }} {
      padding: {{ row_padding_vertical }}px {{ row_padding_horizontal }}px;
    }

    .comparison-grid-rows-{{ section.id }} .comparison-item-{{ section.id }}:first-child h3 {
      font-size: {{ row_heading_size }}px;
    }

    .comparison-value-{{ section.id }} {
      font-size: {{ value_size }}px;
    }

    .comparison-value-icon-{{ section.id }}{
      width: {{ value_icon_size }}px;
      height: {{ value_icon_size }}px;
    }
  }
  
{%- endstyle -%}

{% unless full_width %}
  <style>
    .section-{{ section.id }}-settings {
      max-width: {{ content_width }}px;
    }
  </style>
{% endunless %}

{% if margin_horizontal_mobile > 0 %}
  <style>
    .section-{{ section.id }} {
      border-left: solid {{ border_color }} {{ border_thickness }}px;
      border-right: solid {{ border_color }} {{ border_thickness }}px;
    }
    
    @media(min-width: 1024px) {
      .section-{{ section.id }} {
        border-left: 0px;
        border-right: 0px;
      }
    }
  </style>
{% endif %}

{% if margin_horizontal > 0 %}
  <style>
    @media(min-width: 1024px) {
      .section-{{ section.id }} {
        border-left: solid {{ border_color }} {{ border_thickness }}px;
        border-right: solid {{ border_color }} {{ border_thickness }}px;
      }
    }
  </style>
{% endif %}

{% if heading_custom %}
  <style>
    .comparison-heading-{{ section.id }} * {
      font-family: {{ heading_font.family }}, {{ heading_font.fallback_families }};
      font-weight: {{ heading_font.weight }};
      font-style: {{ heading_font.style }};
    }
  </style>
{% endif %}

{% if text_custom %}
  <style>
    .comparison-text-{{ section.id }} * {
      font-family: {{ text_font.family }}, {{ text_font.fallback_families }};
      font-weight: {{ text_font.weight }};
      font-style: {{ text_font.style }};
    }
  </style>
{% endif %}

{% if column_heading_custom %}
  <style>
    .comparison-grid-header-{{ section.id }} .comparison-column-heading-{{ section.id }} {
      font-family: {{ column_heading_font.family }}, {{ column_heading_font.fallback_families }};
      font-weight: {{ column_heading_font.weight }};
      font-style: {{ column_heading_font.style }};
    }
  </style>
{% endif %}

{% if row_heading_custom %}
  <style>
    .comparison-grid-rows-{{ section.id }} .comparison-item-{{ section.id }}:first-child h3 {
      font-family: {{ row_heading_font.family }}, {{ row_heading_font.fallback_families }};
      font-weight: {{ row_heading_font.weight }};
      font-style: {{ row_heading_font.style }};
    }
  </style>
{% endif %}

{% if value_custom %}
  <style>
    .comparison-value-{{ section.id }} {
      font-family: {{ value_font.family }}, {{ value_font.fallback_families }};
      font-weight: {{ value_font.weight }};
      font-style: {{ value_font.style }};
    }
  </style>
{% endif %}

{% for block in section.blocks %}
  {% case block.type %}
    {% when 'text' %}
      <style>
        {{  block.settings.text_font | font_face: font_display: 'swap' }}

        .comparison-text-{{ block.id }} {
          align-items: center;
          gap: 12px;         
          margin-top: {{ block.settings.text_mt_mobile }}px;
