    }
  </style>
{% endif %}

{% if table_right_custom %}
  <style>
    .pricing-card-table-right-{{ section.id }} {
      font-family: {{ table_right_font.family }}, {{ table_right_font.fallback_families }};
      font-weight: {{ table_right_font.weight }};
      font-style: {{ table_right_font.style }};
    }
  </style>
{% endif %}

{% if features_title_custom %}
  <style>
    .pricing-features-title-{{ section.id }} {
      font-family: {{ features_title_font.family }}, {{ features_title_font.fallback_families }};
      font-weight: {{ features_title_font.weight }};
      font-style: {{ features_title_font.style }};
    }
  </style>
{% endif %}

{% if tab_shadow %}
  <style>
    .pricing-toggle-{{ section.id }}.active {
      box-shadow: rgba(0, 0, 0, 0.08) 0px 1px 3px 0px;
    }
  </style>
{% endif %}

{% for block in section.blocks %}
  {% case block.type %}
    {% when 'card' %}
      <style>
        .pricing-card-{{ block.id }} {
          background-color: {{ block.settings.card_bg_color }};
          border: {{ card_border_thickness }}px solid {{ block.settings.card_border_color }};
        }

        .pricing-card-{{ block.id }} .pricing-card-table-{{ section.id }} {
          border-top-color: {{ block.settings.table_border_color }};
        }

        .pricing-title-{{ block.id }} {
          color: {{ block.settings.title_color }};
        }

        .pricing-text-{{ block.id }} * {
          color: {{ block.settings.text_color }};
        }

        .pricing-price-{{ block.id }} .pricing-price-value-{{ block.id }} {
          color: {{ block.settings.price_text_color }};
        }

        .pricing-price-{{ block.id }} .pricing-price-suffix-{{ block.id }} {
          color: {{ block.settings.price_suffix_text_color }};
        }

        .pricing-card-button-{{ block.id }} {
          color: {{ block.settings.button_color }};
        }

        .pricing-card-button-{{ block.id }}:hover {
          color: {{ block.settings.button_hover_color }};
        }

        .pricing-card-button-{{ block.id }} svg path {
          fill: {{ block.settings.button_color }};
        }

        .pricing-card-button-{{ block.id }}:hover svg path {
          fill: {{ block.settings.button_hover_color }};
        }

        .pricing-card-table-item-{{ block.id }} {
          border-top: {{ table_border_thickness }}px solid {{ block.settings.table_border_color }};
        }

        .pricing-card-table-right-{{ block.id }} {
          color: {{ block.settings.table_right_color }};
        }

        .pricing-card-table-left-{{ block.id }} {
          color: {{ block.settings.table_left_color }};
        }

        .pricing-features-title-{{ block.id }} {
          color: {{ block.settings.features_title_color }};
        }

        .pricing-feature-text-{{ block.id }} {
          color: {{ block.settings.feature_text_color }};
        }

        .pricing-feature-icon-{{ block.id }} {
          color: {{ block.settings.feature_icon_color }};
        }

        .pricing-feature-item-{{ section.id }}:nth-child(2) .pricing-feature-text-{{ block.id }} {
          color: {{ block.settings.first_icon_color }};
        }

        .pricing-feature-item-{{ section.id }}:nth-child(3) .pricing-feature-text-{{ block.id }} {
          color: {{ block.settings.second_icon_color }};
        }

        .pricing-feature-item-{{ section.id }}:nth-child(4) .pricing-feature-text-{{ block.id }} {
          color: {{ block.settings.third_icon_color }};
        }

        .pricing-feature-item-{{ section.id }}:nth-child(5) .pricing-feature-text-{{ block.id }} {
          color: {{ block.settings.fourth_icon_color }};
        }

        .pricing-feature-item-{{ section.id }}:nth-child(6) .pricing-feature-text-{{ block.id }} {
          color: {{ block.settings.fifth_icon_color }};
        }

        .pricing-feature-item-{{ section.id }}:nth-child(7) .pricing-feature-text-{{ block.id }} {
          color: {{ block.settings.sixth_icon_color }};
        }

        .pricing-feature-item-{{ section.id }}:nth-child(8) .pricing-feature-text-{{ block.id }} {
          color: {{ block.settings.seventh_icon_color }};
        }

        .pricing-feature-item-{{ section.id }}:nth-child(9) .pricing-feature-text-{{ block.id }} {
          color: {{ block.settings.eighth_icon_color }};
        }

        .pricing-feature-item-{{ section.id }}:nth-child(10) .pricing-feature-text-{{ block.id }} {
          color: {{ block.settings.ninth_icon_color }};
        }

        .pricing-feature-item-{{ section.id }}:nth-child(11) .pricing-feature-text-{{ block.id }} {
          color: {{ block.settings.tenth_icon_color }};
