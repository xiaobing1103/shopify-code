{%- liquid 
  assign padding_horizontal = section.settings.padding_horizontal
  assign padding_horizontal_mobile = section.settings.padding_horizontal_mobile
  assign padding_top = section.settings.padding_top
  assign padding_bottom = section.settings.padding_bottom
  assign border_color = section.settings.border_color
  assign border_thickness = section.settings.border_thickness
  assign margin_top = section.settings.margin_top
  assign margin_bottom = section.settings.margin_bottom
  assign margin_horizontal_mobile = section.settings.margin_horizontal_mobile
  assign margin_horizontal = section.settings.margin_horizontal
  assign background_color = section.settings.background_color
  assign background_gradient = section.settings.background_gradient
  assign width = section.settings.width
  assign content_width = section.settings.content_width
  assign lazy = section.settings.lazy
  assign section_radius = section.settings.section_radius 
  assign background_style = section.settings.background_style

  assign columns_desktop = section.settings.columns_desktop
  assign columns_per_side = columns_desktop | divided_by: 2

  assign layout_opacity = section.settings.layout_opacity
  assign layout_mt = section.settings.layout_mt
  assign layout_mt_mobile = section.settings.layout_mt_mobile

  assign content_align = section.settings.content_align
  assign content_align_mobile = section.settings.content_align_mobile  

  assign content_horizontal_align = ""
  if content_align == "center"
    assign content_horizontal_align = "center"
  elsif content_align == "right"
    assign content_horizontal_align = "end"
  else
    assign content_horizontal_align = "start"
  endif

  assign content_horizontal_align_mobile = ""
  if content_align_mobile == "center"
    assign content_horizontal_align_mobile = "center"
  elsif content_align_mobile == "right"
    assign content_horizontal_align_mobile = "end"
  else
    assign content_horizontal_align_mobile = "start"
  endif

  assign layout_liquid_glass = section.settings.layout_liquid_glass
  assign show_center_image = section.settings.show_center_image
  assign center_image = section.settings.center_image
  assign center_image_size = section.settings.center_image_size
  assign center_image_size_mobile = section.settings.center_image_size_mobile
  assign center_image_radius = section.settings.center_image_radius
  assign center_image_ratio = section.settings.center_image_ratio
  assign center_image_ratio_mobile = section.settings.center_image_ratio_mobile
  assign center_image_border_thickness = section.settings.center_image_border_thickness
  assign center_image_border_color = section.settings.center_image_border_color

  assign layout_radius = section.settings.layout_radius
  assign layout_border_thickness = section.settings.layout_border_thickness
  assign layout_bg_color = section.settings.layout_bg_color
  assign layout_border_color = section.settings.layout_border_color
  assign layout_padding_vertical = section.settings.layout_padding_vertical
  assign layout_padding_vertical_mobile = section.settings.layout_padding_vertical_mobile
  assign layout_padding_horizontal = section.settings.layout_padding_horizontal
  assign layout_padding_horizontal_mobile = section.settings.layout_padding_horizontal_mobile

  assign icon_size = section.settings.icon_size
  assign icon_size_mobile = section.settings.icon_size_mobile
  assign check_icon_color = section.settings.check_icon_color
  assign cross_icon_color = section.settings.cross_icon_color
  assign check_icon_bg = section.settings.check_icon_bg
  assign cross_icon_bg = section.settings.cross_icon_bg
  assign icon_radius = section.settings.icon_radius
  assign custom_check_icon = section.settings.custom_check_icon
  assign custom_cross_icon = section.settings.custom_cross_icon

  assign text_custom = section.settings.text_custom
  assign text_font = section.settings.text_font
  assign text_size = section.settings.text_size
  assign text_size_mobile = section.settings.text_size_mobile
  assign text_height = section.settings.text_height
  assign text_color = section.settings.text_color

  assign item_padding = section.settings.item_padding
  assign item_padding_mobile = section.settings.item_padding_mobile
  assign item_border_thickness = section.settings.item_border_thickness
  assign item_border_color = section.settings.item_border_color

  assign loading_attribute = 'lazy'
  unless lazy
    assign loading_attribute = 'eager'
  endunless
-%}

{%- style -%}
  {{ text_font | font_face: font_display: 'swap' }}

  .section-{{ section.id }} {
    border-top: solid {{ border_color }} {{ border_thickness }}px;
    border-bottom: solid {{ border_color }} {{ border_thickness }}px;
    margin-top: {{ margin_top | times: 0.75 | round: 0 }}px;
    margin-bottom: {{ margin_bottom | times: 0.75 | round: 0 }}px;
    margin-left: {{ margin_horizontal_mobile }}rem;
    margin-right: {{ margin_horizontal_mobile }}rem;
    border-radius: {{ section_radius | times: 0.6 | round: 0 }}px;
    overflow: hidden;
  }
  
  .section-{{ section.id }}-settings {
    margin: 0 auto;
    padding-top: {{ padding_top | times: 0.75 | round: 0 }}px;
    padding-bottom: {{ padding_bottom | times: 0.75 | round: 0 }}px;
    padding-left: {{ padding_horizontal_mobile }}rem;
    padding-right: {{ padding_horizontal_mobile }}rem;
  }

  .comparison-wrapper-{{ section.id }} {
    position: relative;
    display: flex;
    align-items: center;
    justify-content: center;
    margin-top: {{ layout_mt_mobile }}px;
  }

  .comparison-layout-{{ section.id }} {
    display: grid;
    grid-template-columns: 1fr 1fr;
    width: 100%;
    background-color: {{ layout_bg_color | hex_to_rgba: layout_opacity }};
    border: {{ layout_border_thickness }}px solid {{ layout_border_color }};
    border-radius: {{ layout_radius }}px;
    padding: {{ layout_padding_vertical_mobile }}px {{ layout_padding_horizontal_mobile }}px;
    position: relative;
    gap: 60px;
  }

  .comparison-layout-{{ section.id }}:after {
    content: "";
    width: {{ item_border_thickness }}px;
    height: calc(100% - {{ layout_padding_vertical_mobile | times: 2 }}px);
    background-color: {{ item_border_color }};
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    position: absolute;
  }

  .comparison-side-{{ section.id }} {
    display: flex;
    flex-direction: column;
    gap: 0;
    z-index: 2;
    position: relative;
  }
  
  .comparison-bottom-{{ section.id }} {
    display: flex;
    flex-direction: column;
    align-items: {{ content_horizontal_align_mobile }};
  }

  .comparison-center-image-{{ section.id }} {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    width: {{ center_image_size_mobile }}px;
    height: {{ center_image_size_mobile }}px;
    border-radius: {{ center_image_radius }}px;
    border: {{ center_image_border_thickness }}px solid {{ center_image_border_color }};
    overflow: hidden;
    z-index: 2;
  }

  .comparison-center-image-{{ section.id }} img,
  .comparison-center-image-{{ section.id }} svg {
    width: 100%;
    height: 100%;
    object-fit: cover;
    display: block;
  }

  .comparison-center-image-{{ section.id }} svg {
    background-color: #AFAFAF;
  }

  .comparison-item-{{ section.id }} {
    display: flex;
    flex-direction: column;
    align-items: center;
    gap: 12px;
    padding: {{ item_padding_mobile }}px;
    text-align: center;
    justify-content: start;
  }

  @media(max-width: 320px) {
    .comparison-item-{{ section.id }} {
      flex-grow: 1;
    }
  }

  .comparison-icon-{{ section.id }} {
    width: {{ icon_size_mobile }}px;
    height: {{ icon_size_mobile }}px;
    border-radius: {{ icon_radius | times: 0.5 }}%;
    display: flex;
    align-items: center;
    justify-content: center;
    flex-shrink: 0;
  }

  .comparison-icon-{{ section.id }}.check {
    background-color: {{ check_icon_bg }};
  }

  .comparison-icon-{{ section.id }}.cross {
    background-color: {{ cross_icon_bg }};
  }

  .comparison-icon-{{ section.id }} svg {
    width: 60%;
    height: 60%;
  }

  .comparison-icon-{{ section.id }}.check svg path {
    stroke: {{ check_icon_color }};
  }

  .comparison-icon-{{ section.id }}.cross svg path {
    stroke: {{ cross_icon_color }};
  }

  .comparison-icon-{{ section.id }} img {
    width: 100%;
    height: 100%;
    object-fit: contain;
  }

  .comparison-text-{{ section.id }} {
    margin: 0;
    font-size: {{ text_size_mobile }}px;
    color: {{ text_color }};
    line-height: {{ text_height }}%;
    word-break: break-word;
  }

  @media(max-width: 1024px) {
    .comparison-items-{{ section.id }} .comparison-item-{{ section.id }}:nth-child(odd) {
      border-right: {{ item_border_thickness }}px solid {{ item_border_color }};
    }

    .comparison-side-{{ section.id }} .comparison-item-{{ section.id }}:not(:last-child) {
      border-bottom: {{ item_border_thickness }}px solid {{ item_border_color }};
    }
  }
  

  @media(min-width: 1024px) {
    .section-{{ section.id }} {
      margin-top: {{ margin_top }}px;
      margin-bottom: {{ margin_bottom }}px;
      margin-left: {{ margin_horizontal }}rem;
      margin-right: {{ margin_horizontal }}rem;
      border-radius: {{ section_radius }}px;
    }
    
    .section-{{ section.id }}-settings {
      padding: 0 5rem;
      padding-top: {{ padding_top }}px;
      padding-bottom: {{ padding_bottom }}px;
      padding-left: {{ padding_horizontal }}rem;
      padding-right: {{ padding_horizontal }}rem;
    }

     .comparison-items-{{ section.id }} {
        display: grid;
        grid-template-columns: repeat({{ columns_desktop }}, 1fr);
      }

    .comparison-wrapper-{{ section.id }} {
        margin-top: {{ layout_mt }}px;
    }

    .comparison-layout-{{ section.id }} {
      padding: {{ layout_padding_vertical }}px {{ layout_padding_horizontal }}px;      
    }

    {% if columns_desktop == 2 %}
      .comparison-items-{{ section.id }} .comparison-item-{{ section.id }}:nth-child(odd) {
        border-right: {{ item_border_thickness }}px solid {{ item_border_color }};
      }

      .comparison-items-{{ section.id }} .comparison-item-{{ section.id }}:not(:nth-last-child(-n+2)) {
        border-bottom: {{ item_border_thickness }}px solid {{ item_border_color }};
      }
    {% endif %}

    {% if columns_desktop > 2 %}
      .comparison-items-{{ section.id }} .comparison-item-{{ section.id }}:not(:nth-child({{ columns_desktop }}n)) {
        border-right: {{ item_border_thickness }}px solid {{ item_border_color }};
      }
    {% endif %}

    .comparison-center-image-{{ section.id }} {
      width: {{ center_image_size }}px;
      height: {{ center_image_size }}px;
    }

    .comparison-item-{{ section.id }} {
      padding: {{ item_padding }}px;
    }

    .comparison-layout-{{ section.id }}:after {
      height: calc(100% - {{ layout_padding_vertical | times: 2 }}px);
    }

    .comparison-icon-{{ section.id }} {
      width: {{ icon_size }}px;
      height: {{ icon_size }}px;
    }

    .comparison-text-{{ section.id }} {
      font-size: {{ text_size }}px;
    }

    .comparison-bottom-{{ section.id }} {
      align-items: {{ content_horizontal_align }};
    }

    .comparison-side-{{ section.id }} {
      display: grid;
      grid-template-columns: repeat({{ columns_per_side }}, 1fr);
    }

    {% comment %} Borders {% endcomment %}
    .comparison-side-{{ section.id }} .comparison-item-{{ section.id }} {
      border-bottom: 0;
    }

    {% if columns_per_side > 1 %}
      .comparison-left-{{ section.id }} .comparison-item-{{ section.id }}:not(:nth-child({{ columns_per_side }}n)) {
        border-right: {{ item_border_thickness }}px solid {{ item_border_color }};
      }
      
      .comparison-right-{{ section.id }} .comparison-item-{{ section.id }}:not(:nth-child({{ columns_per_side }}n)) {
        border-right: {{ item_border_thickness }}px solid {{ item_border_color }};
      }

      .comparison-left-{{ section.id }} .comparison-item-{{ section.id }}:nth-last-child(n+{{ columns_per_side | plus: 1 }}) {
        border-bottom: {{ item_border_thickness }}px solid {{ item_border_color }};
      }
      
      .comparison-right-{{ section.id }} .comparison-item-{{ section.id }}:nth-last-child(n+{{ columns_per_side | plus: 1 }}) {
        border-bottom: {{ item_border_thickness }}px solid {{ item_border_color }};
      }
    {% else %}
      .comparison-side-{{ section.id }} .comparison-item-{{ section.id }}:not(:last-child) {
        border-bottom: {{ item_border_thickness }}px solid {{ item_border_color }};
      }
    {% endif %}
  }
{%- endstyle -%}

{% if width == 'custom' %}
  <style>
    .section-{{ section.id }}-settings {
      max-width: {{ content_width }}px;
    }
  </style>
{% elsif width == 'page' %}
  <style>
    .section-{{ section.id }}-settings {
      max-width: clamp(100px, var(--page-content-width, var(--page-width)), 100%);
    }
  </style>
{% endif %}

{% if margin_horizontal_mobile > 0 %}
  <style>
    .section-{{ section.id }} {
      border-left: solid {{ border_color }} {{ border_thickness }}px;
      border-right: solid {{ border_color }} {{ border_thickness }}px;
    }
    
    @media(min-width: 1024px) {
      .section-{{ section.id }} {
        border-left: 0px;
        border-right: 0px;
      }
    }
  </style>
{% endif %}

{% if margin_horizontal > 0 %}
  <style>
    @media(min-width: 1024px) {
      .section-{{ section.id }} {
        border-left: solid {{ border_color }} {{ border_thickness }}px;
        border-right: solid {{ border_color }} {{ border_thickness }}px;
      }
    }
  </style>
{% endif %}

{% if text_custom %}
  <style>
    .comparison-text-{{ section.id }} {
      font-family: {{ text_font.family }}, {{ text_font.fallback_families }};
      font-weight: {{ text_font.weight }};
      font-style: {{ text_font.style }};
    }
  </style>
{% endif %}

{% if layout_liquid_glass %}
  <style>
    .liquid-design-{{ section.id }} {
      position: relative; 
      overflow: hidden;
      z-index: 10;      
      background-color: transparent !important; 
      border-color: transparent !important;
      border-width: 0 !important;
      will-change: transform;
      transform: translateZ(0);
    }

    .liquid-design-{{ section.id }} .glass-filter-{{ section.id }},
    .liquid-design-{{ section.id }} .glass-overlay-{{ section.id }},
    .liquid-design-{{ section.id }} .glass-specular-{{ section.id }} {
        position: absolute;
        inset: 0;
        border-radius: inherit; 
    }

    .liquid-design-{{ section.id }} .glass-filter-{{ section.id }} {
        backdrop-filter: blur(4px);
        -webkit-backdrop-filter: blur(4px);
        filter: url(#lensFilter-{{ section.id }}) saturate(120%) brightness(1.15);
        z-index: 0; 
    }

    .liquid-design-{{ section.id }} .glass-overlay-{{ section.id }} {
        background: rgba(0, 0, 0, 0.15);
        z-index: 1;
        transition: background 0.25s ease;
    }

    .liquid-design-{{ section.id }} .glass-specular-{{ section.id }} {
        {% assign highlight_color = layout_border_color | color_lighten: 40 | color_modify: 'alpha', 0.75 %}
        box-shadow: inset 1px 1px 0 {{ highlight_color }},
                    inset 0 0 5px {{ highlight_color }};
        z-index: 2;
        transition: transform 0.25s ease, background 0.25s ease, box-shadow 0.25s ease;
    }
  </style>
{% endif %}

{% if center_image_ratio_mobile == "portrait" %}
  <style>
    .comparison-center-image-{{ section.id }} {
      aspect-ratio: 9.6/13;
      height: auto;
    }
  </style>
{% elsif center_image_ratio_mobile == "landscape" %}
  <style>
    .comparison-center-image-{{ section.id }} {
      aspect-ratio: 12/8;
      height: auto;
    }
  </style>
{% elsif center_image_ratio_mobile == "square" %}
  <style>
    .comparison-center-image-{{ section.id }} {
      aspect-ratio: 1/1;
      height: auto;
    }
  </style>
{% endif %}

{% if center_image_ratio == "portrait" %}
  <style>
    @media(min-width: 1024px) {
      .comparison-center-image-{{ section.id }} {
        aspect-ratio: 9.6/13;
        height: auto;
      }
    }
  </style>
{% elsif center_image_ratio == "landscape" %}
  <style>
    @media(min-width: 1024px) {
      .comparison-center-image-{{ section.id }} {
        aspect-ratio: 12/8;
        height: auto;
      }
    }
  </style>
{% elsif center_image_ratio == "square" %}
  <style>
    @media(min-width: 1024px) {
      .comparison-center-image-{{ section.id }} {
        aspect-ratio: 1/1;
        height: auto;
      }
    }
  </style>
{% endif %}

{% for block in section.blocks %}
  {% case block.type %}
    {% when 'heading' %}  
      <style>     
        .hero-heading-{{ block.id }} {
          position: relative;
          text-align: {{ content_align_mobile }};
          margin-top: {{ block.settings.heading_mt_mobile }}px;
        }
      
        .hero-heading-{{ block.id }} * {
          margin: 0;
          font-size: {{ block.settings.heading_size_mobile }}px;
          color: {{ block.settings.heading_color }};
          line-height: {{ block.settings.heading_height }}%;
          text-transform: unset;
          word-break: break-word;
          font-weight: 700;
        }

        .hero-heading-{{ block.id }} strong {
          color: {{ block.settings.heading_accent_color }};
        }

        @media(min-width: 1024px) {
          
          .hero-heading-{{ block.id }} {
            text-align: {{ content_align }};
            margin-top: {{ block.settings.heading_mt }}px;
          }
        
          .hero-heading-{{ block.id }} * {
            font-size: {{ block.settings.heading_size }}px;
          }
        }          
      </style>
        
      {% if block.settings.heading_custom %}
        <style>
          {{  block.settings.heading_font | font_face: font_display: 'swap' }}
          {{  block.settings.heading_accent_font | font_face: font_display: 'swap' }}

          .hero-heading-{{ block.id }} * {
            font-family: {{ block.settings.heading_font.family }}, {{ block.settings.heading_font.fallback_families }};
            font-weight: {{ block.settings.heading_font.weight }};
            font-style: {{ block.settings.heading_font.style }};
          }

          .hero-heading-{{ block.id }} strong {
            font-family: {{ block.settings.heading_accent_font.family }}, {{ block.settings.heading_accent_font.fallback_families }};
            font-weight: {{ block.settings.heading_accent_font.weight }};
            font-style: {{ block.settings.heading_accent_font.style }};
          }
        </style>
      {% endif %}

    {% when 'verified' %}
      <style>
        {{  block.settings.text_font | font_face: font_display: 'swap' }}
        .hero-verified-{{ block.id }} {
          display: flex;
          align-items: center;
          gap: 8px;
          justify-content: {{ content_horizontal_align_mobile }};
        }

        .hero-verified-icon-{{ block.id }} {
          flex-shrink: 0;
          width: {{ block.settings.icon_size_mobile }}px;
          height: {{ block.settings.icon_size_mobile }}px;
        }

        .hero-verified-icon-{{ block.id }} img,
        .hero-verified-icon-{{ block.id }} svg {
          display: block;
          width: 100%;
          height: 100%;
          object-fit: cover;
        }

        .hero-verified-{{ block.id }} {
          position: relative;
          text-align: {{ content_align_mobile }};
          margin-top: {{ block.settings.text_mt_mobile }}px;
        }
      
        .hero-verified-{{ block.id }} * {
          margin: 0;
          font-size: {{ block.settings.text_size_mobile }}px;
          color: {{ block.settings.text_color }};
          line-height: {{ block.settings.text_height }}%;
          text-transform: unset;
        }

        .hero-verified-icon-{{ block.id }} svg path {
          fill: {{ block.settings.icon_color }};
        }

        .hero-verified-icon-{{ block.id }} svg circle {
          fill: {{ block.settings.icon_color }};
        }

        .hero-verified-icon-{{ block.id }} .refresh-{{ section.id }} path,
        .hero-verified-icon-{{ block.id }} .refresh-{{ section.id }} polyline {
          fill: none;
          stroke: {{ block.settings.icon_color }};
        }

        @media(min-width: 1024px) {        
          .hero-verified-{{ block.id }} {
            justify-content: {{ content_horizontal_align }};
          }

          .hero-verified-icon-{{ block.id }} {
            width: {{ block.settings.icon_size }}px;
            height: {{ block.settings.icon_size }}px;
          }

          .hero-verified-{{ block.id }} {
            text-align: {{ content_align }};
            margin-top: {{ block.settings.text_mt }}px;
          }
        
          .hero-verified-{{ block.id }} * {
            font-size: {{ block.settings.text_size }}px;
          }
        }
      </style>

       {% if block.settings.text_custom %}
        <style>
          .hero-verified-{{ block.id }} * {
            font-family: {{ block.settings.text_font.family }}, {{ block.settings.text_font.fallback_families }};
            font-weight: {{ block.settings.text_font.weight }};
            font-style: {{ block.settings.text_font.style }};
          }
        </style>
      {% endif %} 

    {% when 'button' %}
      <style>
        {{  block.settings.button_font | font_face: font_display: 'swap' }}
        
        .hero-button-{{ block.id }} {
          position: relative;
          text-decoration: none;
          margin-top: {{ block.settings.button_mt_mobile }}px !important;
          {% if block.settings.button_full_width_mobile %}
            width: 100%;
          {% else %}
            width: fit-content;
          {% endif %}
        }
        
        .hero-button-inner-{{ block.id }} {
          display: flex;
          align-items: center;
          justify-content: center;
          {% if block.settings.button_full_width_mobile %}
            max-width: 100%;
          {% else %}
            max-width: fit-content;
          {% endif %}
          gap: 10px;
          width: 100%;
          margin: 0;          
          font-size: {{ block.settings.button_size_mobile }}px;
          color: {{ block.settings.button_color }};
          line-height: {{ block.settings.button_height }}%;
          text-align: center;
          text-transform: unset;
          text-decoration: none;
          padding: {{ block.settings.button_padding_vertical_mobile }}px {{ block.settings.button_padding_horizontal_mobile }}px;
          border-radius: {{ block.settings.button_radius }}px;
          transition: all 0.25s ease 0s;
          background-color: transparent;
          border: 0px;
          cursor: pointer;
          {% if block.settings.button_custom_styling and block.settings.button_style != "link" %}
            position: relative;
            overflow: hidden;
          {% endif %}
        }

        {% if block.settings.button_custom_styling and block.settings.button_style != "link" %}
          .hero-button-inner-{{ block.id }} {
            background: linear-gradient(145deg, {{ block.settings.button_bg_color }}, {{ block.settings.button_bg_color | color_darken: 7.5 }});
            box-shadow: 0 8px 16px {{ block.settings.button_shadow_color }}4d, 0 4px 8px {{ block.settings.button_shadow_color }}33, inset 0 1px 0 #ffffff1a, inset 0 -1px 0 {{ block.settings.button_shadow_color }}4d;
            backdrop-filter: saturate(1.05);
            transition: all 0.25s ease 0s;
          }

          .hero-button-inner-{{ block.id }}::before {
            content: '';
            position: absolute;
            top: 0;
            left: -100%;
            width: 100%;
            height: 100%;
            background: linear-gradient(90deg, transparent, rgba(255, 255, 255, 0.1), transparent);
            transition: left 0.6s ease;
          }

          .hero-button-inner-{{ block.id }}:hover::before {
            left: 100%;
          }

          .hero-button-inner-{{ block.id }}:hover {
            color: {{ block.settings.button_hover_color }};
            background: linear-gradient(145deg, {{ block.settings.button_bg_hover_color }}, {{ block.settings.button_bg_hover_color | color_darken: 7.5 }});
            transform: translateY(-2px);
            transition: all 0.25s ease 0s;
            box-shadow: 0 12px 24px {{ block.settings.button_shadow_color }}66, 0 6px 12px {{ block.settings.button_shadow_color }}4d, inset 0 1px 0 #ffffff26, inset 0 -1px 0 {{ block.settings.button_shadow_color }}66;
          }

          .hero-button-inner-{{ block.id }}:active {
            transform: translateY(1px);
            filter: brightness(1.00208);
            box-shadow: 0 4px 8px {{ block.settings.button_shadow_color }}4d, 0 2px 4px {{ block.settings.button_shadow_color }}33, inset 0 1px 0 #ffffff0d, inset 0 -1px 0 {{ block.settings.button_shadow_color }}33;
          }
        {% else %}
          .hero-button-inner-{{ block.id }}:hover {
            color: {{ block.settings.button_hover_color }};
            transition: all 0.25s ease 0s;
          }
        {% endif %}

        .hero-button-inner-{{ block.id }} svg {
          width: 14px !important;
          height: 14px !important;
          background: transparent;
          min-height: auto !important;
        }

        .hero-button-inner-{{ block.id }} svg path {
          fill: {{ block.settings.button_color }};
          transition: all 0.25s ease 0s;
        }

        .hero-button-inner-{{ block.id }}:hover svg path {
          fill: {{ block.settings.button_hover_color }};
          transition: all 0.25s ease 0s;
        }

        @media(min-width: 1024px) {        
          .hero-button-{{ block.id }} {
            margin-top: {{ block.settings.button_mt }}px !important;
            width: fit-content;
          }

          .hero-button-inner-{{ block.id }} {
            display: flex !important;
            max-width: fit-content;            
            padding: {{ block.settings.button_padding_vertical }}px {{ block.settings.button_padding_horizontal }}px;
            font-size: {{ block.settings.button_size }}px;
          }
        }
      </style>

       {% if block.settings.button_custom %}
        <style>
          .hero-button-inner-{{ block.id }} {
            font-family: {{ block.settings.button_font.family }}, {{ block.settings.button_font.fallback_families }};
            font-weight: {{ block.settings.button_font.weight }};
            font-style: {{ block.settings.button_font.style }};
          }
        </style>
      {% endif %} 
  
      {% if block.settings.button_style == "non_outline" or block.settings.button_style == "non_outline_arrow" %}
        <style>
          .hero-button-inner-{{ block.id }} {
            background-color: {{ block.settings.button_bg_color }};
          }
  
          .hero-button-inner-{{ block.id }}:hover {
            background-color: {{ block.settings.button_bg_hover_color }};
          }
        </style>
      {% elsif block.settings.button_style == "outline" or block.settings.button_style == "outline_arrow" %}
        <style>
          .hero-button-inner-{{ block.id }} {
            background-color: {{ block.settings.button_bg_color }};
            border: {{ block.settings.button_border_thickness }}px solid {{ block.settings.button_border_color }};
          }
  
          .hero-button-inner-{{ block.id }}:hover {
            background-color: {{ block.settings.button_bg_hover_color }};
            border: {{ block.settings.button_border_thickness }}px solid {{ block.settings.button_border_hover_color }};
          }
        </style>
      {% elsif block.settings.button_style == "link" %}
        <style>
          .hero-button-inner-{{ block.id }} {
            padding: 0px !important;
          }
        </style>
      {% endif %}   

    {% when 'item' %}
      {% if block.settings.hide_on_mobile %}
        <style>
          @media(max-width: 1024px) {
            .comparison-item-{{ block.id }} {
              display: none;
            }          
          }
        </style>
      {% endif %}
  {% endcase %}
{% endfor %}


