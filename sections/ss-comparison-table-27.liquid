<script>
  function initComparisonTable27() {
    const items = document.querySelectorAll('.comparison-item-{{ section.id }}');

    items.forEach(item => {
      item.style.height = 'auto';
    });

    if (window.innerWidth < 1024) {
      let maxHeight = 0;

      items.forEach(item => {
        if (item.offsetHeight > maxHeight) {
          maxHeight = item.offsetHeight;
        }
      });

      if (maxHeight > 0) {
        items.forEach(item => {
          item.style.height = maxHeight + 'px';
        });
      }
    }
  }

  document.addEventListener('DOMContentLoaded', initComparisonTable27);
  window.addEventListener('load', initComparisonTable27);
  window.addEventListener('resize', initComparisonTable27);

  if (Shopify.designMode) {
    document.addEventListener('shopify:section:unload', initComparisonTable27);
    document.addEventListener('shopify:section:load', initComparisonTable27);
    document.addEventListener('shopify:section:select', initComparisonTable27);
  }
</script>

