                    {% if custom_cross_icon != blank %}
                      <img src="{{ custom_cross_icon | image_url: width: 200 }}" alt="{{ block.settings.text | escape }}" loading="{{ loading_attribute }}">
                    {% else %}
                      <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="3" stroke="currentColor">
                        <path stroke-linecap="round" stroke-linejoin="round" d="M6 18L18 6M6 6l12 12" />
                      </svg>
                    {% endif %}
                  </div>
                  {% if block.settings.text != blank %}
                    <p class="comparison-text-{{ section.id }}">{{ block.settings.text }}</p>
                  {% endif %}
                </div>
              {% endif %}
            {% endfor %}
          </div>

