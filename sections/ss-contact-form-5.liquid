                          </svg>
                        </div>
                      </div>
                    {% when 'text_field' %}
                      {% if block.settings.title != blank %}
                        <p class="contact-field-title-{{ section.id }} contact-field-title-{{ block.id }}">
                          {{ block.settings.title }}
                          {% if block.settings.required -%}
                            <span class="contact-field-required-{{ section.id }}">{{ required_text }}</span>
                          {%- endif %}
                        </p>
                      {% endif %}
                      <div class="contact-field-inner-{{ section.id }}">
                        {% if block.settings.label != blank %}
                          <label
                            for="ContactForm{{ block.settings.name }}"
                            class="contact-field-label-{{ section.id }} contact-field-label-{{ block.id }}"
                          >
                            {{ block.settings.label }}
                          </label>
                        {% endif %}
                        <input
                          type="{% if block.settings.type == "tel" %}tel{% elsif block.settings.type == "email" %}email{% else %}text{% endif %}"
                          {% if block.settings.placeholder != blank %}
                            placeholder="{{ block.settings.placeholder }}"
                          {% endif %}
                          class="contact-field-input-{{ section.id }}"
                          id="ContactForm{{ block.settings.name }}"
                          name="{% if block.settings.type == "email" %}contact[email]{% else %}contact[{{ block.settings.name }}]{% endif %}"
                          {% if block.settings.required %}
                            required
                          {% endif %}
                        >
                      </div>
                    {% when 'textarea' %}
