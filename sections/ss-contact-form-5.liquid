      padding: {{ submit_padding_vertical }}px {{ submit_padding_horizontal }}px;
    }

    .contact-image-content-{{ section.id }} {
      justify-content: {{ image_content_vertical_align }};
      align-items: {{ image_content_horizontal_align }};
    }
  }
