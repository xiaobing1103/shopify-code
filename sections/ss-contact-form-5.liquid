                                {% if block.settings.placeholder != blank %}{{ block.settings.placeholder }}{% endif %}
                              </option>
                            {% endif %}

                            {% for option in options %}
                              <option value="{{option}}">{{ option }}</option>
                            {% endfor %}
                          </select>
                        </div>
                      </div>
                    {% when 'checkbox' %}
