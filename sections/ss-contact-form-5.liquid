                            <input
                              type="checkbox"
                              id="ContactForm{{ block.settings.name }}{{ forloop.index }}"
                              name="{% if block.settings.use_newsletter and forloop.first %}customer[accepts_marketing]{% else %}contact[{{ block.settings.name }}-{{ option }}]{% endif %}"
                            >
