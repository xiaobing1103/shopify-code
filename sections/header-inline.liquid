          <div class="t4s-col-3 t4s-d-lg-none t4s-order-1 t4s-text-start t4s-col-item">{%- render 't4s_logo', tag: 'div', isTransparent: h_transparent, page_type_logo: page_type_logo -%}</div>
          <div class="t4s-col-9 t4s-d-lg-none t4s-order-2 t4s-text-end t4s-col-group_btns t4s-col-item t4s-lh-1">
