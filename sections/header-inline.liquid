{%- liquid
  assign se_stts = section.settings
  assign se_blocks = section.blocks 
  assign admin_sp = request.design_mode
  assign h__bgimg = se_stts.h__bgimg
  assign h_transparent = se_stts.transparent_header
  if request.page_type != 'index' 
    assign h_transparent = false 
  endif
  assign space_transparent = se_stts.space_transparent_header
  assign mobile_layout = se_stts.mobile_layout | default: 'logo_left'
  
  # 根据页面类型自动调整移动端布局
  if request.page_type == 'index' and se_stts.mobile_layout_home != blank and se_stts.mobile_layout_home != ''
    assign mobile_layout = se_stts.mobile_layout_home
  elsif request.page_type == 'product' and se_stts.mobile_layout_product != blank and se_stts.mobile_layout_product != ''
    assign mobile_layout = se_stts.mobile_layout_product
  elsif request.page_type == 'collection' and se_stts.mobile_layout_collection != blank and se_stts.mobile_layout_collection != ''
    assign mobile_layout = se_stts.mobile_layout_collection
  endif
-%}

{%- style -%}
  {%- assign opnav  = se_stts.opnav | divided_by: 100.0 -%}
  .t4s-header__wrapper {
    --h-text-color      : {{ se_stts.clnav }};
    --h-text-color-rgb  : {{ se_stts.clnav | color_to_rgb | remove: 'rgba(' | remove: 'rgb(' | remove: ')' }};
    --h-text-color-hover: {{ se_stts.clnav_hover }};
    --h-bg-color        : {{ se_stts.bgnav | color_modify: 'alpha', opnav }};
    background-color: var(--h-bg-color) !important;
  }
  .t4s-header__wrapper * {
    color: var(--h-text-color);
  }
  .t4s-header__wrapper a:hover,
  .t4s-header__wrapper button:hover {
    color: var(--h-text-color-hover) !important;
  }
  .t4s-count-box {
    --h-count-bgcolor: {{ se_stts.bg_hc }};
    --h-count-color: {{ se_stts.cl_hc }}; 
  }

  {%- if h__bgimg != blank %}
  .t4s-header__bgimg {
      background-size: cover;
      background-repeat: no-repeat;
  }
  {%- endif -%}

  {%- if h_transparent -%}

    .t4s-section-header,#shopify-section-top-bar { --h-space-tr: {% if space_transparent %}30px{% else %}0px{% endif %} }

    {%- assign opnavtr  = se_stts.opnavtr | divided_by: 100.0 -%}
    .t4s-header__wrapper {
      --h-text-color      : {{ se_stts.clnavtr }};
      --h-text-color-rgb  : {{ se_stts.clnavtr | color_to_rgb | remove: 'rgba(' | remove: 'rgb(' | remove: ')' }};
      --h-text-color-hover: {{ se_stts.clnavtr_hover }};
      --h-bg-color        : {{ se_stts.bgnavtr | color_modify: 'alpha', opnavtr }};
    }
    .t4s-section-header {
        margin-top: var(--h-space-tr);
        margin-bottom: calc(-1 * (var(--header-height) + var(--h-space-tr)));
        position: relative;
        top: 0;
        z-index: 460;
    }
    {%- if space_transparent -%}
      .is--topbar-transparent.is--header-transparent #shopify-section-top-bar {
        margin-top: var(--h-space-tr);
      }
      #t4s-hsticky__sentinel {
        bottom:calc(-1 * var(--h-space-tr));
      }
    {%- endif -%}

  {%- endif -%}

  {%- if se_stts.sticky_header -%}

    {%- unless se_stts.scroll_header -%}
    .t4s-hsticky__ready .t4s-section-header {
        position: sticky;      
        top: 0;
        z-index: 460;
    }
    .is-header--stuck .t4s-section-header {
      -webkit-box-shadow: 0 1px 3px rgb(0 0 0 / 10%);
      box-shadow: 0 1px 3px rgb(0 0 0 / 10%);
    }
    {%- endunless -%}
    
    {%- assign opnavst  = se_stts.opnavst | divided_by: 100.0 -%}
    .is-header--stuck .t4s-header__wrapper {
      --h-text-color      : {{ se_stts.clnavst }};
      --h-text-color-rgb  : {{ se_stts.clnavst | color_to_rgb | remove: 'rgba(' | remove: 'rgb(' | remove: ')' }};
      --h-text-color-hover: {{ se_stts.clnavst_hover }};
      --h-bg-color        : {{ se_stts.bgnavst | color_modify: 'alpha', opnavst }};
    }
    .is-header--stuck .header__sticky-logo {
      display:block !important
    }
    .is-header--stuck .header__normal-logo,
    .is-header--stuck .header__mobile-logo {
      display:none !important
    }
  {%- endif -%}
 
  .t4s-section-header [data-header-height] {
      min-height: {{ se_stts.h_navmb }}px;    
  }
  .t4s-header__logo img {
    padding-top: 5px;
    padding-bottom: 5px;
    transform: translateZ(0);
    max-height: inherit;
    height: auto;
    width: 100%;
    max-width: 100%;
  }
  .t4s-header__logo img[src*=".svg"] {
    height: 100%;
    perspective: 800px;
    -webkit-perspective: 800px;
    backface-visibility: hidden;
    -webkit-backface-visibility: hidden;
  }
  .t4s-site-nav__icons .t4s-site-nav__icon {
      padding: 0 {{ se_stts.icon_spacing_mobile | default: 6 }}px;
      display: inline-block;
      line-height: 1;
  }
  .t4s-site-nav__icons svg.t4s-icon {
      color: var(--h-text-color);
      line-height: 1;
      vertical-align: middle;
      transition: color 0.2s ease-in-out;
      width: {{ se_stts.icon_size_mobile | default: 22 }}px;
      height: {{ se_stts.icon_size_mobile | default: 22 }}px;
  }
  .t4s-push-menu-btn svg {
      width: {{ se_stts.icon_size_mobile | default: 22 }}px;
      height: {{ se_stts.icon_size_mobile | default: 22 }}px;
  }
  .t4s-site-nav__icons.t4s-use__kalles svg.t4s-icon--account {
      width: 24px;
      height: 24px;
  }
  .t4s-site-nav__icons.t4s-use__line svg.t4s-icon {
    width: 25px;
    height: 25px;
  }
  .t4s-site-nav__icon>a:hover svg.t4s-icon {
      color: var(--h-text-color-hover);
  }
  .t4s-site-nav__icon a { 
    display: inline-block;
    line-height: 1;
  }
  .t4s-site-nav__cart >a,.t4s-push-menu-btn {color: var(--h-text-color)}
  .t4s-site-nav__cart >a:hover {color: var(--h-text-color-hover)}
  
  @media (max-width: 1024px) {
    .t4s-col-group_btns {
      display: flex !important;
      align-items: center;
    }
    .t4s-site-nav__icons {
      display: flex !important;
      align-items: center;
    }
    .t4s-mobile-icon-search { 
      order: {{ se_stts.mobile_search_order | default: 1 }} !important; 
    }
    .t4s-mobile-icon-cart { 
      order: {{ se_stts.mobile_cart_order | default: 2 }} !important; 
    }
    .t4s-mobile-icon-menu { 
      order: {{ se_stts.mobile_menu_order | default: 3 }} !important; 
    }
    .t4s-mobile-icon-menu .t4s-push-menu-btn {
      padding-top: {{ se_stts.menu_padding_top | default: 0 }}px;
      padding-bottom: {{ se_stts.menu_padding_bottom | default: 0 }}px;
    }
  }
  
  @media (min-width: 768px) {
    .t4s-site-nav__icons .t4s-site-nav__icon {
        padding: 0 {{ se_stts.icon_spacing_desktop | default: 8 }}px;
    }
    .t4s-site-nav__icons svg.t4s-icon {
        width: {{ se_stts.icon_size_desktop | default: 22 }}px;
        height: {{ se_stts.icon_size_desktop | default: 22 }}px;
    }
    .t4s-push-menu-btn svg {
        width: {{ se_stts.icon_size_desktop | default: 22 }}px;
        height: {{ se_stts.icon_size_desktop | default: 22 }}px;
    }
  }
  @media (min-width: 1025px) {
      .t4s-section-header [data-header-height] {
         min-height: {{ se_stts.height }}px;    
      }
      {%- if se_stts.full_header -%}
      .t4s-announcement-bar >.t4s-container, .t4s-top-bar >.t4s-container, .t4s-header__wrapper >.t4s-container {
          max-width: 100%;
      }
      .t4s-header-layout_logo_left .t4s-header__logo {
          margin-left: 10px;
      }
      {%- else -%}
         {%- if space_transparent and h_transparent -%}
          .is--topbar-transparent.is--header-transparent #shopify-section-top-bar,
          html:not(.is-header--stuck) .t4s-section-header {
            max-width: 1170px;
            margin-right: auto;
            margin-left: auto;
          }
        {%- endif -%}
        .t4s-header__wrapper>.t4s-container {
          padding-right: 20px;
          padding-left: 20px;
        }
      {%- endif -%}
      .t4s-nav__ul {
          margin: 0;padding:0
      }
    .t4s-nav__ul>li> a {
        color: var(--h-text-color);
        padding: 5px 17.5px;
        text-transform: none;
        font-family: var(--font-family-{{ se_stts.fm_nav }});
        font-weight: {{ se_stts.fw_nav }};
        font-size: {{ se_stts.fs_nav }}px;
        {%- if se_stts.ls_nav != 0 %}letter-spacing: {{ se_stts.ls_nav }}px;{% endif -%}
    }
    .t4s-nav__ul>li> a:hover { color: var(--h-text-color-hover) !important}
    .t4s-header-layout_logo_left .t4s-col-group_btns {
        min-width: 16.666667%;
    }
    .t4s-nav__ul .t4s-icon-select-arrow {
        position: static;
        width: 8px;
        margin-inline-start: 4px;
        height: 8px;
        opacity: .8;
    }
    .t4s-header-layout_logo_center .t4s-nav__ul>li> a {
           padding-inline-start: 5px;
    }

    li.t4s-menu-item a {
    font-size: {{section.settings.item_fsize}}px;
    padding: calc(0.5 * var({{ section.settings.sp_bettewn_item }},16px)) 0;
    }

    {%- if se_stts.enable_active %}
    .t4s-nav__ul>li.is--nav__active> a {
      color: var(--h-text-color-hover) !important;
      transition: none;
    }
    {%- endif -%}
  }
  .shopify-section-header-sticky [data-header-height] {
    position: relative;
  }
{%- endstyle -%}

<div data-header-options='{ "isTransparent": {{ h_transparent }},"isSticky": {{ se_stts.sticky_header }},"hideScroldown": {{ se_stts.scroll_header }} }' class="t4s-header__wrapper t4s-pr t4s-header-layout_{{ se_stts.layout }} {% if h__bgimg != blank %} lazyloadt4s t4s-header__bgimg" data-bgset="{{ h__bgimg | image_url: width: 1 }}" data-ratio="{{ h__bgimg.aspect_ratio }}" data-sizes="auto"{% else %}"{% endif %}>
  <div class="t4s-container">
    <div data-header-height class="t4s-row t4s-gx-15 t4s-gx-md-30 t4s-align-items-center t4s-justify-content-between">
    {%- case se_stts.layout -%}

      {%- when 'logo_left' -%}
        {%- if mobile_layout == 'logo_left' -%}
          <div class="t4s-col-lg-2 t4s-col-md-4 t4s-col-3 t4s-order-1 t4s-text-start t4s-col-item">{%- render 't4s_logo', tag: 'div', isTransparent: h_transparent -%}</div>
          <div class="t4s-col t4s-d-none t4s-d-lg-block t4s-order-2 t4s-col-item">{%- render 'menu_blocks', placement: 'bottom', admin_sp: admin_sp, se_blocks: se_blocks, se_stts: se_stts -%}</div>
          <div class="t4s-col-lg-auto t4s-col-md-4 t4s-col-9 t4s-order-3 t4s-text-end t4s-col-group_btns t4s-col-item t4s-lh-1 t4s-d-flex t4s-justify-content-end t4s-align-items-center">
            {%- render 't4s_group_btns', se_stts: se_stts, showMobileMenu: true -%}
          </div>
        {%- elsif mobile_layout == 'logo_center' -%}
          <div class="t4s-col-lg-2 t4s-d-none t4s-d-lg-block t4s-order-lg-1 t4s-text-lg-start t4s-col-item">{%- render 't4s_logo', tag: 'div', isTransparent: h_transparent -%}</div>
          <div class="t4s-col t4s-d-none t4s-d-lg-block t4s-order-lg-2 t4s-col-item">{%- render 'menu_blocks', placement: 'bottom', admin_sp: admin_sp, se_blocks: se_blocks, se_stts: se_stts -%}</div>
          <div class="t4s-col-3 t4s-d-lg-none t4s-order-1 t4s-text-start t4s-col-item t4s-col-group_btns">
            {%- render 't4s_group_btns', se_stts: se_stts, showMobileMenu: true, onlyMenu: true -%}
          </div>
          <div class="t4s-col-6 t4s-d-lg-none t4s-order-2 t4s-text-center t4s-col-item">{%- render 't4s_logo', tag: 'div', isTransparent: h_transparent -%}</div>
          <div class="t4s-col-lg-auto t4s-col-3 t4s-order-3 t4s-text-end t4s-col-group_btns t4s-col-item t4s-lh-1">{%- render 't4s_group_btns', se_stts: se_stts -%}</div>
        {%- else -%}
          <div class="t4s-col-lg-2 t4s-d-none t4s-d-lg-block t4s-order-lg-1 t4s-text-lg-start t4s-col-item">{%- render 't4s_logo', tag: 'div', isTransparent: h_transparent -%}</div>
          <div class="t4s-col t4s-d-none t4s-d-lg-block t4s-order-lg-2 t4s-col-item">{%- render 'menu_blocks', placement: 'bottom', admin_sp: admin_sp, se_blocks: se_blocks, se_stts: se_stts -%}</div>
          <div class="t4s-col-lg-auto t4s-col-6 t4s-order-1 t4s-order-lg-3 t4s-text-start t4s-text-lg-end t4s-col-group_btns t4s-col-item t4s-lh-1">
            {%- render 't4s_group_btns', se_stts: se_stts, showMobileMenu: true -%}
          </div>
          <div class="t4s-col-6 t4s-d-lg-none t4s-order-2 t4s-text-end t4s-col-item">{%- render 't4s_logo', tag: 'div', isTransparent: h_transparent -%}</div>
        {%- endif -%}
    
      {%- when 'logo_center' -%}
        {%- if mobile_layout == 'logo_left' -%}
          <div class="t4s-col-5 t4s-d-none t4s-d-lg-block t4s-order-lg-1 t4s-col-item">{%- render 'menu_blocks', placement: 'bottom', admin_sp: admin_sp, se_blocks: se_blocks, se_stts: se_stts -%}</div>
          <div class="t4s-col-lg-2 t4s-d-none t4s-d-lg-block t4s-order-lg-2 t4s-text-lg-center t4s-col-item">{%- render 't4s_logo', tag: 'div', isTransparent: h_transparent -%}</div>
          <div class="t4s-col-lg-5 t4s-d-none t4s-d-lg-block t4s-order-lg-3 t4s-text-lg-end t4s-col-group_btns t4s-col-item t4s-lh-1">{%- render 't4s_group_btns', se_stts: se_stts -%}</div>
          <div class="t4s-col-3 t4s-d-lg-none t4s-order-1 t4s-text-start t4s-col-item">{%- render 't4s_logo', tag: 'div', isTransparent: h_transparent -%}</div>
          <div class="t4s-col-9 t4s-d-lg-none t4s-order-2 t4s-text-end t4s-col-group_btns t4s-col-item t4s-lh-1">
            {%- render 't4s_group_btns', se_stts: se_stts, showMobileMenu: true -%}
          </div>
        {%- elsif mobile_layout == 'logo_center' -%}
          <div class="t4s-col-5 t4s-d-none t4s-d-lg-block t4s-order-lg-1 t4s-col-item">{%- render 'menu_blocks', placement: 'bottom', admin_sp: admin_sp, se_blocks: se_blocks, se_stts: se_stts -%}</div>
          <div class="t4s-col-lg-2 t4s-col-6 t4s-order-2 t4s-text-center t4s-col-item">{%- render 't4s_logo', tag: 'div', isTransparent: h_transparent -%}</div>
          <div class="t4s-col-3 t4s-d-lg-none t4s-order-1 t4s-text-start t4s-col-item t4s-col-group_btns">
            {%- render 't4s_group_btns', se_stts: se_stts, showMobileMenu: true, onlyMenu: true -%}
          </div>
          <div class="t4s-col-lg-5 t4s-col-3 t4s-order-3 t4s-text-end t4s-col-group_btns t4s-col-item t4s-lh-1">{%- render 't4s_group_btns', se_stts: se_stts -%}</div>
        {%- else -%}
          <div class="t4s-col-5 t4s-d-none t4s-d-lg-block t4s-order-lg-1 t4s-col-item">{%- render 'menu_blocks', placement: 'bottom', admin_sp: admin_sp, se_blocks: se_blocks, se_stts: se_stts -%}</div>
          <div class="t4s-col-lg-2 t4s-d-none t4s-d-lg-block t4s-order-lg-2 t4s-text-lg-center t4s-col-item">{%- render 't4s_logo', tag: 'div', isTransparent: h_transparent -%}</div>
          <div class="t4s-col-lg-5 t4s-d-none t4s-d-lg-block t4s-order-lg-3 t4s-text-lg-end t4s-col-group_btns t4s-col-item t4s-lh-1">{%- render 't4s_group_btns', se_stts: se_stts -%}</div>
          <div class="t4s-col-6 t4s-d-lg-none t4s-order-1 t4s-text-start t4s-col-group_btns t4s-col-item t4s-lh-1">
            {%- render 't4s_group_btns', se_stts: se_stts, showMobileMenu: true -%}
          </div>
          <div class="t4s-col-6 t4s-d-lg-none t4s-order-2 t4s-text-end t4s-col-item">{%- render 't4s_logo', tag: 'div', isTransparent: h_transparent -%}</div>
        {%- endif -%}
      
      {%- when 'menu_split' -%}
        {%- if mobile_layout == 'logo_left' -%}
          <div class="t4s-col-lg-2 t4s-d-lg-block t4s-d-none t4s-order-lg-1 t4s-col-group_btns t4s-col-item t4s-lh-1">{%- render 't4s_group_btns_split', se_stts: se_stts, isSplit1: true -%}</div>
          <div class="t4s-col t4s-d-none t4s-d-lg-block t4s-order-lg-2 t4s-col-item">{%- render 'menu_splits', placement: 'bottom', admin_sp: admin_sp, se_blocks: se_blocks, se_stts: se_stts, isSplit1: true -%}</div>
          <div class="t4s-col-lg-auto t4s-d-none t4s-d-lg-block t4s-order-lg-3 t4s-text-lg-center t4s-col-item">{%- render 't4s_logo', tag: 'div', isTransparent: h_transparent -%}</div>
          <div class="t4s-col t4s-d-none t4s-d-lg-block t4s-order-lg-4 t4s-col-item">{%- render 'menu_splits', placement: 'bottom', admin_sp: admin_sp, se_blocks: se_blocks, se_stts: se_stts, isSplit1: false -%}</div>
          <div class="t4s-col-lg-2 t4s-d-none t4s-d-lg-block t4s-order-lg-5 t4s-text-lg-end t4s-col-group_btns t4s-col-item t4s-lh-1">{%- render 't4s_group_btns_split', se_stts: se_stts, isSplit1: false -%}</div>
          <div class="t4s-col-3 t4s-d-lg-none t4s-order-1 t4s-text-start t4s-col-item">{%- render 't4s_logo', tag: 'div', isTransparent: h_transparent -%}</div>
          <div class="t4s-col-9 t4s-d-lg-none t4s-order-2 t4s-text-end t4s-col-group_btns t4s-col-item t4s-lh-1 t4s-d-flex t4s-justify-content-end t4s-align-items-center">
            <div class="t4s-me-auto">{%- render 'push_menu' -%}</div>
            {%- render 't4s_group_btns_split', se_stts: se_stts, isSplit1: false -%}
          </div>
        {%- elsif mobile_layout == 'logo_center' -%}
          <div class="t4s-col-lg-2 t4s-d-lg-block t4s-d-none t4s-order-lg-1 t4s-col-group_btns t4s-col-item t4s-lh-1">{%- render 't4s_group_btns_split', se_stts: se_stts, isSplit1: true -%}</div>
          <div class="t4s-col t4s-d-none t4s-d-lg-block t4s-order-lg-2 t4s-col-item">{%- render 'menu_splits', placement: 'bottom', admin_sp: admin_sp, se_blocks: se_blocks, se_stts: se_stts, isSplit1: true -%}</div>
          <div class="t4s-col-lg-auto t4s-col-6 t4s-order-2 t4s-text-center t4s-col-item">{%- render 't4s_logo', tag: 'div', isTransparent: h_transparent -%}</div>
          <div class="t4s-col t4s-d-none t4s-d-lg-block t4s-order-lg-4 t4s-col-item">{%- render 'menu_splits', placement: 'bottom', admin_sp: admin_sp, se_blocks: se_blocks, se_stts: se_stts, isSplit1: false -%}</div>
          <div class="t4s-col-3 t4s-d-lg-none t4s-order-1 t4s-text-start t4s-col-item">{%- render 'push_menu' -%}</div>
          <div class="t4s-col-lg-2 t4s-col-3 t4s-order-3 t4s-text-end t4s-col-group_btns t4s-col-item t4s-lh-1">{%- render 't4s_group_btns_split', se_stts: se_stts, isSplit1: false -%}</div>
        {%- else -%}
          <div class="t4s-col-lg-2 t4s-d-lg-block t4s-d-none t4s-order-lg-1 t4s-col-group_btns t4s-col-item t4s-lh-1">{%- render 't4s_group_btns_split', se_stts: se_stts, isSplit1: true -%}</div>
          <div class="t4s-col t4s-d-none t4s-d-lg-block t4s-order-lg-2 t4s-col-item">{%- render 'menu_splits', placement: 'bottom', admin_sp: admin_sp, se_blocks: se_blocks, se_stts: se_stts, isSplit1: true -%}</div>
          <div class="t4s-col-lg-auto t4s-d-none t4s-d-lg-block t4s-order-lg-3 t4s-text-lg-center t4s-col-item">{%- render 't4s_logo', tag: 'div', isTransparent: h_transparent -%}</div>
          <div class="t4s-col t4s-d-none t4s-d-lg-block t4s-order-lg-4 t4s-col-item">{%- render 'menu_splits', placement: 'bottom', admin_sp: admin_sp, se_blocks: se_blocks, se_stts: se_stts, isSplit1: false -%}</div>
