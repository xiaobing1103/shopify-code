          <div class="t4s-col-lg-2 t4s-d-none t4s-d-lg-block t4s-order-lg-2 t4s-text-lg-center t4s-col-item">{%- render 't4s_logo', tag: 'div', isTransparent: h_transparent, page_type_logo: page_type_logo -%}</div>
