{%- liquid
  assign se_stts = section.settings
  assign se_blocks = section.blocks 
  assign admin_sp = request.design_mode
  assign h__bgimg = se_stts.h__bgimg
  assign h_transparent = false
  assign space_transparent = false
  assign mobile_layout = se_stts.mobile_layout | default: 'logo_left'
  
  # 根据页面类型自动调整移动端布局
  if request.page_type == 'index' and se_stts.mobile_layout_home != blank and se_stts.mobile_layout_home != ''
    assign mobile_layout = se_stts.mobile_layout_home
  elsif request.page_type == 'product' and se_stts.mobile_layout_product != blank and se_stts.mobile_layout_product != ''
    assign mobile_layout = se_stts.mobile_layout_product
  elsif request.page_type == 'collection' and se_stts.mobile_layout_collection != blank and se_stts.mobile_layout_collection != ''
    assign mobile_layout = se_stts.mobile_layout_collection
  endif

  # 根据页面类型选择颜色（简单方式 - 直接从 section settings 读取）
  assign final_bg_color = se_stts.bgnav
  assign final_text_color = se_stts.clnav
  assign final_text_hover_color = se_stts.clnav_hover
  assign page_type_logo = blank
  
  # 如果启用了页面类型特定颜色，则使用对应的颜色和 logo
  if se_stts.enable_page_colors
    case request.page_type
      when 'product'
        if se_stts.product_bgnav != blank
          assign final_bg_color = se_stts.product_bgnav
        endif
        if se_stts.product_clnav != blank
          assign final_text_color = se_stts.product_clnav
        endif
        if se_stts.product_clnav_hover != blank
          assign final_text_hover_color = se_stts.product_clnav_hover
        endif
        assign page_type_logo = se_stts.product_logo
      when 'collection'
        if se_stts.collection_bgnav != blank
          assign final_bg_color = se_stts.collection_bgnav
        endif
        if se_stts.collection_clnav != blank
          assign final_text_color = se_stts.collection_clnav
        endif
        if se_stts.collection_clnav_hover != blank
          assign final_text_hover_color = se_stts.collection_clnav_hover
        endif
        assign page_type_logo = se_stts.collection_logo
      when 'page'
        if se_stts.page_bgnav != blank
          assign final_bg_color = se_stts.page_bgnav
        endif
        if se_stts.page_clnav != blank
          assign final_text_color = se_stts.page_clnav
        endif
        if se_stts.page_clnav_hover != blank
          assign final_text_hover_color = se_stts.page_clnav_hover
        endif
        assign page_type_logo = se_stts.page_logo
      when 'article'
        if se_stts.article_bgnav != blank
          assign final_bg_color = se_stts.article_bgnav
        endif
        if se_stts.article_clnav != blank
          assign final_text_color = se_stts.article_clnav
        endif
        if se_stts.article_clnav_hover != blank
          assign final_text_hover_color = se_stts.article_clnav_hover
        endif
        assign page_type_logo = se_stts.article_logo
    endcase
  endif
  
  # 优先级最高：单个产品/集合/页面的 Metafield 设置
  # 如果当前是产品页面，检查产品的自定义 metafield
  if request.page_type == 'product' and product != blank
    if product.metafields.custom.header_bg_color != blank
      assign final_bg_color = product.metafields.custom.header_bg_color
    endif
    if product.metafields.custom.header_text_color != blank
      assign final_text_color = product.metafields.custom.header_text_color
    endif
    if product.metafields.custom.header_text_hover_color != blank
      assign final_text_hover_color = product.metafields.custom.header_text_hover_color
    endif
    if product.metafields.custom.header_logo != blank
      assign page_type_logo = product.metafields.custom.header_logo
    endif
  endif
  
  # 如果是集合页面，检查集合的自定义 metafield
  if request.page_type == 'collection' and collection != blank
    if collection.metafields.custom.header_bg_color != blank
      assign final_bg_color = collection.metafields.custom.header_bg_color
    endif
    if collection.metafields.custom.header_text_color != blank
      assign final_text_color = collection.metafields.custom.header_text_color
    endif
    if collection.metafields.custom.header_text_hover_color != blank
      assign final_text_hover_color = collection.metafields.custom.header_text_hover_color
    endif
    if collection.metafields.custom.header_logo != blank
      assign page_type_logo = collection.metafields.custom.header_logo
    endif
  endif
  
  # 如果是普通页面，检查页面的自定义 metafield
  if request.page_type == 'page' and page != blank
    if page.metafields.custom.header_bg_color != blank
      assign final_bg_color = page.metafields.custom.header_bg_color
    endif
    if page.metafields.custom.header_text_color != blank
      assign final_text_color = page.metafields.custom.header_text_color
    endif
    if page.metafields.custom.header_text_hover_color != blank
      assign final_text_hover_color = page.metafields.custom.header_text_hover_color
    endif
    if page.metafields.custom.header_logo != blank
      assign page_type_logo = page.metafields.custom.header_logo
    endif
  endif
-%}

{%- style -%}
  {%- assign opnav  = se_stts.opnav | divided_by: 100.0 -%}
  
  {%- comment -%} 处理背景颜色：如果透明度是100%，直接使用原始值（支持渐变），否则应用透明�?{%- endcomment -%}
  {%- if se_stts.opnav == 100 -%}
    {%- assign final_bg_with_opacity = final_bg_color -%}
  {%- else -%}
    {%- assign final_bg_with_opacity = final_bg_color | color_modify: 'alpha', opnav -%}
  {%- endif -%}
  
  .t4s-header__wrapper {
    --h-text-color      : {{ final_text_color }};
    --h-text-color-rgb  : {{ final_text_color | color_to_rgb | remove: 'rgba(' | remove: 'rgb(' | remove: ')' }};
    --h-text-color-hover: {{ final_text_hover_color }};
    --h-bg-color        : {{ final_bg_with_opacity }};
    background: var(--h-bg-color) !important;
  }
  .t4s-header__wrapper * {
    color: var(--h-text-color);
  }
  .t4s-header__wrapper a:hover,
  .t4s-header__wrapper button:hover {
    color: var(--h-text-color-hover) !important;
  }
  
  {%- comment -%} 下拉菜单和子菜单使用独立的颜色，不继�?header 的文字颜�?{%- endcomment -%}
  .t4s-header__wrapper .t4s-sub-menu,
  .t4s-header__wrapper .t4s-sub-menu *,
  .t4s-header__wrapper .t4s-dropdown__wrapper,
  .t4s-header__wrapper .t4s-dropdown__wrapper *,
  .t4s-header__wrapper .t4s-lb__wrapper,
  .t4s-header__wrapper .t4s-lb__wrapper * {
    color: var(--text-color) !important;
  }
  .t4s-header__wrapper .t4s-sub-menu a:hover,
  .t4s-header__wrapper .t4s-dropdown__wrapper a:hover,
  .t4s-header__wrapper .t4s-lb__wrapper a:hover {
    color: var(--primary-color) !important;
  }
  .t4s-header__wrapper .t4s-sub-menu .type_mn_link2 > a:not(:hover),
  .t4s-header__wrapper .t4s-sub-menu .type_mn_link > .t4s-heading,
  .t4s-header__wrapper .t4s-sub-menu .nt_menu .sub-menu .sub-column-item>a {
    color: var(--secondary-color) !important;
  }
  
  {%- comment -%} 确保下拉菜单背景色正�?{%- endcomment -%}
  .t4s-header__wrapper .t4s-sub-menu,
  .t4s-header__wrapper .t4s-dropdown__wrapper,
  .t4s-header__wrapper .t4s-lb__wrapper {
    background-color: var(--t4s-light-color) !important;
  }
  .t4s-count-box {
    --h-count-bgcolor: {{ se_stts.bg_hc }};
    --h-count-color: {{ se_stts.cl_hc }}; 
  }

  {%- if h__bgimg != blank %}
  .t4s-header__bgimg {
      background-size: cover;
      background-repeat: no-repeat;
  }
  {%- endif -%}

  {%- comment -%} Transparent header removed - always use page-type colors {%- endcomment -%}

  {%- comment -%} Header position configuration {%- endcomment -%}
  {%- if se_stts.header_position == 'fixed' -%}
  .t4s-section-header {
    position: fixed !important;
    top: 0;
    left: 0;
    right: 0;
    z-index: 460;
    -webkit-box-shadow: 0 1px 3px rgb(0 0 0 / 10%);
    box-shadow: 0 1px 3px rgb(0 0 0 / 10%);
  }
  {%- elsif se_stts.header_position == 'sticky' or se_stts.header_position == 'sticky_hide' -%}
  .t4s-hsticky__ready .t4s-section-header {
        position: sticky;      
        top: 0;
        z-index: 460;
    }
    .is-header--stuck .t4s-section-header {
      -webkit-box-shadow: 0 1px 3px rgb(0 0 0 / 10%);
      box-shadow: 0 1px 3px rgb(0 0 0 / 10%);
    }
    
    {%- comment -%} Sticky header uses page-type colors {%- endcomment -%}
    {%- assign final_bg_sticky = final_bg_color -%}
    {%- if se_stts.opnav != 100 -%}
      {%- assign opnav_sticky = se_stts.opnav | divided_by: 100.0 -%}
      {%- assign final_bg_sticky = final_bg_color | color_modify: 'alpha', opnav_sticky -%}
    {%- endif -%}
    
    .is-header--stuck .t4s-header__wrapper {
      --h-text-color      : {{ final_text_color }};
      --h-text-color-rgb  : {{ final_text_color | color_to_rgb | remove: 'rgba(' | remove: 'rgb(' | remove: ')' }};
      --h-text-color-hover: {{ final_text_hover_color }};
      --h-bg-color        : {{ final_bg_sticky }};
    }
    
    .is-header--stuck .header__sticky-logo {
      display:block !important
    }
    .is-header--stuck .header__normal-logo,
    .is-header--stuck .header__mobile-logo {
      display:none !important
    }
  {%- endif -%}
 
  .t4s-section-header [data-header-height] {
      min-height: {{ se_stts.h_navmb }}px;    
  }
  .t4s-header__logo img {
    padding-top: 5px;
    padding-bottom: 5px;
    transform: translateZ(0);
    max-height: inherit;
    height: auto;
    width: 100%;
    max-width: 100%;
  }
  .t4s-header__logo img[src*=".svg"] {
    height: 100%;
    perspective: 800px;
    -webkit-perspective: 800px;
    backface-visibility: hidden;
    -webkit-backface-visibility: hidden;
  }
  .t4s-site-nav__icons .t4s-site-nav__icon {
      padding: 0 {{ se_stts.icon_spacing_mobile | default: 6 }}px;
      display: inline-block;
      line-height: 1;
  }
  .t4s-site-nav__icons svg.t4s-icon {
      color: var(--h-text-color);
      line-height: 1;
      vertical-align: middle;
      transition: color 0.2s ease-in-out;
      width: {{ se_stts.icon_size_mobile | default: 22 }}px;
      height: {{ se_stts.icon_size_mobile | default: 22 }}px;
  }
  .t4s-push-menu-btn svg {
      width: {{ se_stts.icon_size_mobile | default: 22 }}px;
      height: {{ se_stts.icon_size_mobile | default: 22 }}px;
  }
  .t4s-site-nav__icons.t4s-use__kalles svg.t4s-icon--account {
      width: 24px;
      height: 24px;
  }
  .t4s-site-nav__icons.t4s-use__line svg.t4s-icon {
    width: 25px;
    height: 25px;
  }
  .t4s-site-nav__icon>a:hover svg.t4s-icon {
      color: var(--h-text-color-hover);
  }
  .t4s-site-nav__icon a { 
    display: inline-block;
    line-height: 1;
  }
  .t4s-site-nav__cart >a,.t4s-push-menu-btn {color: var(--h-text-color)}
  .t4s-site-nav__cart >a:hover {color: var(--h-text-color-hover)}
  
  @media (max-width: 1024px) {
    .t4s-col-group_btns {
      display: flex !important;
      align-items: center;
    }
    .t4s-site-nav__icons {
      display: flex !important;
      align-items: center;
    }
    .t4s-mobile-icon-search { 
      order: {{ se_stts.mobile_search_order | default: 1 }} !important; 
    }
    .t4s-mobile-icon-cart { 
      order: {{ se_stts.mobile_cart_order | default: 2 }} !important; 
    }
    .t4s-mobile-icon-menu { 
      order: {{ se_stts.mobile_menu_order | default: 3 }} !important; 
    }
    .t4s-mobile-icon-menu .t4s-push-menu-btn {
      padding-top: {{ se_stts.menu_padding_top | default: 0 }}px;
      padding-bottom: {{ se_stts.menu_padding_bottom | default: 0 }}px;
    }
  }
  
  @media (min-width: 768px) {
    .t4s-site-nav__icons .t4s-site-nav__icon {
        padding: 0 {{ se_stts.icon_spacing_desktop | default: 8 }}px;
    }
    .t4s-site-nav__icons svg.t4s-icon {
        width: {{ se_stts.icon_size_desktop | default: 22 }}px;
        height: {{ se_stts.icon_size_desktop | default: 22 }}px;
    }
    .t4s-push-menu-btn svg {
        width: {{ se_stts.icon_size_desktop | default: 22 }}px;
        height: {{ se_stts.icon_size_desktop | default: 22 }}px;
    }
  }
  @media (min-width: 1025px) {
      .t4s-section-header [data-header-height] {
         min-height: {{ se_stts.height }}px;    
      }
      {%- if se_stts.full_header -%}
      .t4s-announcement-bar >.t4s-container, .t4s-top-bar >.t4s-container, .t4s-header__wrapper >.t4s-container {
          max-width: 100%;
      }
      .t4s-header-layout_logo_left .t4s-header__logo {
          margin-left: 10px;
      }
      {%- else -%}
        .t4s-header__wrapper>.t4s-container {
          padding-right: 20px;
          padding-left: 20px;
        }
      {%- endif -%}
      .t4s-nav__ul {
          margin: 0;padding:0
      }
    .t4s-nav__ul>li> a {
        color: var(--h-text-color);
        padding: 5px 17.5px;
        text-transform: none;
        font-family: var(--font-family-{{ se_stts.fm_nav }});
        font-weight: {{ se_stts.fw_nav }};
        font-size: {{ se_stts.fs_nav }}px;
        {%- if se_stts.ls_nav != 0 %}letter-spacing: {{ se_stts.ls_nav }}px;{% endif -%}
    }
    .t4s-nav__ul>li> a:hover { color: var(--h-text-color-hover) !important}
    .t4s-header-layout_logo_left .t4s-col-group_btns {
        min-width: 16.666667%;
    }
    .t4s-nav__ul .t4s-icon-select-arrow {
        position: static;
        width: 8px;
        margin-inline-start: 4px;
        height: 8px;
        opacity: .8;
    }
    .t4s-header-layout_logo_center .t4s-nav__ul>li> a {
           padding-inline-start: 5px;
    }

    li.t4s-menu-item a {
    font-size: {{section.settings.item_fsize}}px;
    padding: calc(0.5 * var({{ section.settings.sp_bettewn_item }},16px)) 0;
    }

    {%- if se_stts.enable_active %}
    .t4s-nav__ul>li.is--nav__active> a {
      color: var(--h-text-color-hover) !important;
      transition: none;
    }
    {%- endif -%}
  }
  .shopify-section-header-sticky [data-header-height] {
    position: relative;
  }
{%- endstyle -%}

