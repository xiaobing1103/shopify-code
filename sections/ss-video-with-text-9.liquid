{%- liquid
  assign padding_horizontal = section.settings.padding_horizontal
  assign padding_horizontal_mobile = section.settings.padding_horizontal_mobile
  assign padding_top = section.settings.padding_top
  assign padding_bottom = section.settings.padding_bottom
  assign border_color = section.settings.border_color
  assign border_thickness = section.settings.border_thickness
  assign margin_top = section.settings.margin_top
  assign margin_bottom = section.settings.margin_bottom
  assign margin_horizontal_mobile = section.settings.margin_horizontal_mobile
  assign margin_horizontal = section.settings.margin_horizontal
  assign background_color = section.settings.background_color
  assign background_gradient = section.settings.background_gradient
  assign width = section.settings.width
  assign content_width = section.settings.content_width
  assign lazy = section.settings.lazy
  assign section_radius = section.settings.section_radius
  assign background_style = section.settings.background_style

  assign layout_gap_mobile = section.settings.layout_gap_mobile
  assign layout_gap = section.settings.layout_gap

  assign content_horizontal_align_mobile = section.settings.content_horizontal_align_mobile
  assign content_horizontal_align = section.settings.content_horizontal_align
  assign content_vertical_align = section.settings.content_vertical_align

  assign text_align = ''
  if content_horizontal_align == 'start'
    assign text_align = 'left'
  elsif content_horizontal_align == 'center'
    assign text_align = 'center'
  else
    assign text_align = 'right'
  endif

  assign text_align_mobile = ''
  if content_horizontal_align_mobile == 'start'
    assign text_align_mobile = 'left'
  elsif content_horizontal_align_mobile == 'center'
    assign text_align_mobile = 'center'
  else
    assign text_align_mobile = 'right'
  endif

  assign buttons_gap = section.settings.buttons_gap
  assign buttons_gap_mobile = section.settings.buttons_gap_mobile
  assign buttons_mt = section.settings.buttons_mt
  assign buttons_mt_mobile = section.settings.buttons_mt_mobile

  assign open_in_new_tab = section.settings.open_in_new_tab

  assign video = section.settings.video
  assign video_url = section.settings.video_url
  assign source = section.settings.source
  assign video_autoplay = section.settings.video_autoplay
  assign video_loop = section.settings.video_loop
  assign use_controls = section.settings.use_controls
  assign video_position = section.settings.video_position
  assign video_ratio = section.settings.video_ratio
  assign video_ratio_mobile = section.settings.video_ratio_mobile
  assign video_radius = section.settings.video_radius
  assign video_border_thickness = section.settings.video_border_thickness
  assign video_border_color = section.settings.video_border_color

  assign video_mobile_order = section.settings.video_mobile_order

  assign play_icon_size = section.settings.play_icon_size
  assign play_icon_size_mobile = section.settings.play_icon_size_mobile
  assign play_border_thickness = section.settings.play_border_thickness
  assign play_border_color = section.settings.play_border_color
  assign play_border_hover_color = section.settings.play_border_hover_color
  assign play_padding = section.settings.play_padding
  assign play_padding_mobile = section.settings.play_padding_mobile
  assign play_radius = section.settings.play_radius
  assign play_bg_color = section.settings.play_bg_color
  assign play_color = section.settings.play_color
  assign play_bg_hover_color = section.settings.play_bg_hover_color
  assign play_hover_color = section.settings.play_hover_color

  assign features_per_row = section.settings.features_per_row
  assign features_per_row_mobile = section.settings.features_per_row_mobile

  assign feature_icon_size = section.settings.feature_icon_size
  assign feature_icon_size_mobile = section.settings.feature_icon_size_mobile
  assign feature_icon_padding = section.settings.feature_icon_padding
  assign feature_icon_border_thickness = section.settings.feature_icon_border_thickness
  assign feature_icon_border_color = section.settings.feature_icon_border_color
  assign feature_icon_radius = section.settings.feature_icon_radius
  assign feature_icon_color = section.settings.feature_icon_color

  assign feature_title_size = section.settings.feature_title_size
  assign feature_title_size_mobile = section.settings.feature_title_size_mobile
  assign feature_title_color = section.settings.feature_title_color
  assign feature_title_custom = section.settings.feature_title_custom
  assign feature_title_font = section.settings.feature_title_font
  assign feature_title_height = section.settings.feature_title_height

  assign feature_text_size = section.settings.feature_text_size
  assign feature_text_size_mobile = section.settings.feature_text_size_mobile
  assign feature_text_color = section.settings.feature_text_color
  assign feature_text_custom = section.settings.feature_text_custom
  assign feature_text_font = section.settings.feature_text_font
  assign feature_text_height = section.settings.feature_text_height
  assign feature_text_mt = section.settings.feature_text_mt
  assign feature_text_mt_mobile = section.settings.feature_text_mt_mobile

  assign loading_attribute = 'eager'
  if lazy
    assign loading_attribute = 'lazy'
  endif
-%}

{%- style -%}
  {{ feature_title_font | font_face: font_display: 'swap' }}
  {{ feature_text_font | font_face: font_display: 'swap' }}

  .section-{{ section.id }} {
    border-top: solid {{ border_color }} {{ border_thickness }}px;
    border-bottom: solid {{ border_color }} {{ border_thickness }}px;
    margin-top: {{ margin_top | times: 0.75 | round: 0 }}px;
    margin-bottom: {{ margin_bottom | times: 0.75 | round: 0 }}px;
    margin-left: {{ margin_horizontal_mobile }}rem;
    margin-right: {{ margin_horizontal_mobile }}rem;
    border-radius: {{ section_radius | times: 0.6 | round: 0 }}px;
    overflow: hidden;
  }

  .section-{{ section.id }}-settings {
    margin: 0 auto;
    padding-top: {{ padding_top | times: 0.75 | round: 0 }}px;
    padding-bottom: {{ padding_bottom | times: 0.75 | round: 0 }}px;
    padding-left: {{ padding_horizontal_mobile }}rem;
    padding-right: {{ padding_horizontal_mobile }}rem;
  }

  .video-text-body-{{ section.id }} {
    display: flex;
    flex-direction: column;
    gap: {{ layout_gap_mobile }}px;
  }

  .video-text-content-{{ section.id }} {
    display: contents;
  }

  @media(min-width: 1024px) {
    .video-text-content-{{ section.id }} {
      display: flex;
      flex-direction: column;
      align-items: {{ content_horizontal_align }};
    }
  }

  .video-text-video-{{ section.id }} {
    position: relative;
    overflow: hidden;
    border-radius: {{ video_radius }}px;
    border: {{ video_border_thickness }}px solid {{ video_border_color }};
  }

  .video-text-video-{{ section.id }} video,
  .video-text-video-{{ section.id }} iframe {
    display: block;
    width: 100%;
    height: 100%;
    object-fit: cover;
    border: 0px;
  }

  .video-text-buttons-{{ section.id }} {
    display: flex;
    align-items: center;
    margin-top: {{ buttons_mt_mobile }}px;
    gap: {{ buttons_gap_mobile }}px;
    flex-wrap: wrap;
    justify-content: {{ content_horizontal_align_mobile }};
  }

  .video-with-text-controls-{{ section.id }} {
    display: flex;
    gap: 8px;
    position: absolute;
    top: 16px;
    left: 16px;
  }

  .video-with-text-play-{{ section.id }},
  .video-with-text-sound-{{ section.id }} {
    margin: 0;
    cursor: pointer;
    z-index: 2;
    padding: {{ play_padding_mobile }}px;
    border: {{ play_border_thickness }}px solid {{ play_border_color }};
    border-radius: {{ play_radius }}px;
    background-color: {{ play_bg_color }};
    transition: all 0.25s ease 0s;
  }

  .video-with-text-play-{{ section.id }} svg:first-child path,
  .video-with-text-sound-{{ section.id }} svg:first-child path {
    fill: {{ play_color }};
  }

  .video-with-text-play-{{ section.id }} svg:last-child path,
  .video-with-text-sound-{{ section.id }} svg:last-child path {
    fill: {{ play_color }};
  }

  .video-with-text-play-{{ section.id }} svg,
  .video-with-text-sound-{{ section.id }} svg {
    display: block;
    width: {{ play_icon_size_mobile }}px;
    height: {{ play_icon_size_mobile }}px;
    object-fit: cover;
  }

  .video-with-text-play-{{ section.id }} svg,
  .video-with-text-sound-{{ section.id }} svg {
    background-color: transparent !important;
  }

  .video-with-text-play-{{ section.id }} svg:nth-child(1),
  .video-with-text-sound-{{ section.id }} svg:nth-child(1){
    display: none;
  }

  .video-with-text-play-{{ section.id }}.active svg:nth-child(1),
  .video-with-text-sound-{{ section.id }}.active svg:nth-child(1) {
    display: block;
  }

  .video-with-text-play-{{ section.id }}.active svg:nth-child(2),
  .video-with-text-sound-{{ section.id }}.active svg:nth-child(2) {
    display: none;
  }


  .video-with-text-feature-icon-{{ section.id }} {
    flex-shrink: 0;
    border: {{ feature_icon_border_thickness }}px solid {{ feature_icon_border_color }};
    padding: {{ feature_icon_padding }}px;
    border-radius: {{ feature_icon_radius }}px;
    overflow: hidden;
    display: flex;
    align-items: center;
    justify-content: center;
  }

  .video-with-text-feature-{{ section.id }}:hover .video-with-text-feature-icon-{{ section.id }} img,
  .video-with-text-feature-{{ section.id }}:hover .video-with-text-feature-icon-{{ section.id }} svg {
    transform: scale(1.1);
    transition: all 0.4s ease 0s;
  }

  .video-with-text-feature-icon-{{ section.id }} img,
  .video-with-text-feature-icon-{{ section.id }} svg {
    display: block;
    width: {{ feature_icon_size_mobile }}px;
    height: {{ feature_icon_size_mobile }}px;
    object-fit: contain;
    transition: all 0.4s ease 0s;
  }

  .video-with-text-feature-icon-{{ section.id }} svg path {
    fill: {{ feature_icon_color }};
  }

  .video-with-text-feature-icon-{{ section.id }} svg circle {
    fill: {{ feature_icon_color }};
  }

  .video-with-text-feature-icon-{{ section.id }} .refresh-{{ section.id }} path,
  .video-with-text-feature-icon-{{ section.id }} .refresh-{{ section.id }} polyline{
    fill: none;
    stroke: {{ feature_icon_color }};
  }

  .video-with-text-feature-title-{{ section.id }} {
    margin: 0px;
    text-align: {{ text_align_mobile }};
    font-size: {{ feature_title_size_mobile }}px;
    color: {{ feature_title_color }};
    line-height: {{ feature_title_height }}%;
    text-transform: unset;
    font-weight: 700;
  }

  .video-with-text-feature-text-{{ section.id }} {
    margin-top: {{ feature_text_mt_mobile }}px;
    text-align: {{ text_align_mobile }};
  }

  .video-with-text-feature-text-{{ section.id }} * {
    margin: 0px;
    font-size: {{ feature_text_size_mobile }}px;
    color: {{ feature_text_color }};
    line-height: {{ feature_text_height }}%;
    text-transform: unset;
    text-wrap: wrap;
    word-break: break-word;
  }

  .video-with-text-feature-{{ section.id }} {
    display: flex;
    flex-direction: column;
    align-items: {{ content_horizontal_align_mobile }};
    gap: 15px;
  }

  .video-with-text-features-{{ section.id }} {
    display: grid;
    grid-template-columns: repeat({{ features_per_row_mobile }}, 1fr);
    gap: 20px;
    margin-top: 20px;
    order: 4;
  }

  @media(min-width: 1024px) {

    .section-{{ section.id }} {
      margin-top: {{ margin_top }}px;
      margin-bottom: {{ margin_bottom }}px;
      margin-left: {{ margin_horizontal }}rem;
      margin-right: {{ margin_horizontal }}rem;
      border-radius: {{ section_radius }}px;
    }

    .section-{{ section.id }}-settings {
      padding: 0 5rem;
      padding-top: {{ padding_top }}px;
      padding-bottom: {{ padding_bottom }}px;
      padding-left: {{ padding_horizontal }}rem;
      padding-right: {{ padding_horizontal }}rem;
    }

    .video-text-body-{{ section.id }} {
      gap: {{ layout_gap }}px;
      flex-direction: row;
    }

    .video-text-video-{{ section.id }} {
      flex: 1;
      min-width: 0;
    }

    .video-text-content-{{ section.id }} {
      flex: 1;
      min-width: 0;
      justify-content: {{ content_vertical_align }};
      align-items: {{ content_horizontal_align }};
    }

    .video-text-buttons-{{ section.id }} {
      margin-top: {{ buttons_mt }}px;
      gap: {{ buttons_gap }}px;
      justify-content: {{ content_horizontal_align }};
    }

    .video-with-text-play-{{ section.id }},
    .video-with-text-sound-{{ section.id }} {
      padding: {{ play_padding }}px;
    }

    .video-with-text-play-{{ section.id }}:hover,
    .video-with-text-sound-{{ section.id }}:hover {
      border: {{ play_border_thickness }}px solid {{ play_border_hover_color }};
      background-color: {{ play_bg_hover_color }};
      transition: all 0.25s ease 0s;
    }

    .video-with-text-play-{{ section.id }}:hover svg:first-child path,
    .video-with-text-sound-{{ section.id }}:hover svg:first-child path {
      fill: {{ play_hover_color }};
    }

    .video-with-text-play-{{ section.id }}:hover svg:last-child path,
    .video-with-text-sound-{{ section.id }}:hover svg:last-child path {
      fill: {{ play_hover_color }};
    }

    .video-with-text-play-{{ section.id }} svg,
    .video-with-text-sound-{{ section.id }} svg {
      width: {{ play_icon_size }}px;
      height: {{ play_icon_size }}px;
    }


    .video-with-text-feature-icon-{{ section.id }} {
      width: {{ feature_icon_size }}px;
      height: {{ feature_icon_size }}px;
      display: flex;
      align-items: center;
      justify-content: center;
    }

    .video-with-text-feature-icon-{{ section.id }} img,
    .video-with-text-feature-icon-{{ section.id }} svg {
      width: 100%;
      height: 100%;
      max-width: {{ feature_icon_size }}px;
      max-height: {{ feature_icon_size }}px;
      object-fit: contain;
    }

    .video-with-text-bottom-menus-wrapper-{{ section.id }} {
      flex-direction: row;
    }

    .video-with-text-feature-title-{{ section.id }} {
      font-size: {{ feature_title_size }}px;
      text-align: {{ text_align }};
    }

    .video-with-text-feature-text-{{ section.id }} {
      margin-top: {{ feature_text_mt }}px;
      text-align: {{ text_align }};
    }

    .video-with-text-feature-text-{{ section.id }} * {
      font-size: {{ feature_text_size }}px;
    }

    .video-with-text-feature-{{ section.id }} {
      align-items: {{ content_horizontal_align }};
    }

    .video-with-text-features-{{ section.id }} {
      display: grid;
      grid-template-columns: repeat({{ features_per_row }}, 1fr);
      gap: 20px;
      margin-top: 20px;
      order: initial;
    }
  }
{%- endstyle -%}

{% if width == 'custom' %}
  <style>
    .section-{{ section.id }}-settings {
      max-width: {{ content_width }}px;
    }
  </style>
{% elsif width == 'page' %}
  <style>
    .section-{{ section.id }}-settings {
      max-width: var(--page-width);
    }
  </style>
{% endif %}

{% if margin_horizontal_mobile > 0 %}
  <style>
    .section-{{ section.id }} {
      border-left: solid {{ border_color }} {{ border_thickness }}px;
      border-right: solid {{ border_color }} {{ border_thickness }}px;
    }

    @media(min-width: 1024px) {
      .section-{{ section.id }} {
        border-left: 0px;
        border-right: 0px;
      }
    }
  </style>
{% endif %}

{% if margin_horizontal > 0 %}
  <style>
    @media(min-width: 1024px) {
      .section-{{ section.id }} {
        border-left: solid {{ border_color }} {{ border_thickness }}px;
        border-right: solid {{ border_color }} {{ border_thickness }}px;
      }
    }
  </style>
{% endif %}

{% if feature_title_custom %}
  <style>
    .video-with-text-feature-title-{{ section.id }} {
      font-family: {{ feature_title_font.family }}, {{ feature_title_font.fallback_families }};
      font-weight: {{ feature_title_font.weight }};
      font-style: {{ feature_title_font.style }};
    }
  </style>
{% endif %}

{% if feature_text_custom %}
  <style>
    .video-with-text-feature-text-{{ section.id }} {
      font-family: {{ feature_text_font.family }}, {{ feature_text_font.fallback_families }};
      font-weight: {{ feature_text_font.weight }};
      font-style: {{ feature_text_font.style }};
    }
  </style>
{% endif %}

{% comment %} Mobile element order {% endcomment %}
<style>
  .video-text-video-{{ section.id }} {
    order: 2;
  }

  @media(min-width: 1024px) {
    .video-text-video-{{ section.id }} {
      order: initial;
    }
  }
</style>

{% if video_position == 'right' %}
  <style>
    @media(min-width: 1024px) {

      .video-text-video-{{ section.id }} {
          order: 2;
      }

      .video-text-content-{{ section.id }} {
          order: 1;
      }
    }
  </style>
{% else %}
  <style>
    @media(min-width: 1024px) {

      .video-text-video-{{ section.id }} {
          order: 1;
      }

      .video-text-content-{{ section.id }} {
          order: 2;
      }
    }
  </style>
{% endif %}

{% if video_ratio_mobile == 'portrait' %}
  <style>
    .video-text-video-{{ section.id }} {
      aspect-ratio: 9.6/12;
    }
  </style>
{% elsif video_ratio_mobile == 'landscape' %}
  <style>
    .video-text-video-{{ section.id }} {
      aspect-ratio: 12/9;
    }
  </style>
{% elsif video_ratio_mobile == 'square' %}
  <style>
    .video-text-video-{{ section.id }} {
      aspect-ratio: 12/12;
    }
  </style>
{% else %}
  {% if source == 'uploaded' %}
    <style>
      .video-text-video-{{ section.id }} {
        aspect-ratio: auto;
      }
    </style>
  {% else %}
    <style>
      .video-text-video-{{ section.id }} {
        aspect-ratio: 16/9;
      }
    </style>
  {% endif %}
{% endif %}

{% if video_ratio == 'portrait' %}
  <style>
    @media(min-width: 1024px) {
     .video-text-video-{{ section.id }} {
        aspect-ratio: 9.6/12;
      }
    }
  </style>
{% elsif video_ratio == 'landscape' %}
  <style>
    @media(min-width: 1024px) {
      .video-text-video-{{ section.id }}{
        aspect-ratio: 12/9;
      }
    }
  </style>
{% elsif video_ratio == 'square' %}
  <style>
    @media(min-width: 1024px) {
      .video-text-video-{{ section.id }} {
        aspect-ratio: 12/12;
      }
    }
  </style>
{% else %}
  {% if source == 'uploaded' %}
    <style>
      @media(min-width: 1024px) {
        .video-text-video-{{ section.id }} {
          aspect-ratio: auto;
        }
      }
    </style>
  {% else %}
    <style>
      @media(min-width: 1024px) {
        .video-text-video-{{ section.id }} {
          aspect-ratio: 16/9;
        }
      }
    </style>
  {% endif %}
{% endif %}

{% for block in section.blocks %}
  {% if block.type == 'heading' %}
    <style>
      {{ block.settings.heading_font | font_face: font_display: 'swap' }}

      .video-text-heading-{{ block.id }} {
        text-align: {{ text_align_mobile }};
        margin-top: {{ block.settings.heading_mt_mobile }}px;
        padding-bottom: 10px;
      }

      .video-text-heading-{{ block.id }} * {
        margin: 0;
        font-size: {{ block.settings.heading_size_mobile }}px;
        color: {{ block.settings.heading_color }};
        line-height: {{ block.settings.heading_height }}%;
        text-transform: unset;
        font-weight: 700;
      }

      .video-text-heading-{{ block.id }} strong {
        {% if block.settings.heading_gradient != blank %}
          background: {{ block.settings.heading_gradient }};
          -webkit-background-clip: text;
          background-clip: text;
          color: transparent;
          -webkit-text-fill-color: transparent;
        {% else %}
          color: {{ block.settings.heading_accent_color }};
        {% endif %}
      }

      @media(min-width: 1024px) {

        .video-text-heading-{{ block.id }} {
          text-align: {{ text_align }};
          margin-top: {{ block.settings.heading_mt }}px;
        }

        .video-text-heading-{{ block.id }} * {
          font-size: {{ block.settings.heading_size }}px;
        }
      }
    </style>

    {% if block.settings.heading_custom %}
      <style>
        .video-text-heading-{{ block.id }} * {
          font-family: {{ block.settings.heading_font.family }}, {{ block.settings.heading_font.fallback_families }};
          font-weight: {{ block.settings.heading_font.weight }};
          font-style: {{ block.settings.heading_font.style }};
        }
      </style>
    {% endif %}

    <style>
      .video-text-heading-{{ block.id }} {
        order: 1;
      }

      @media(min-width: 1024px) {
        .video-text-heading-{{ block.id }} {
          order: initial;
        }
      }
    </style>

  {% elsif block.type == 'text' %}
    <style>
      {{ block.settings.text_font | font_face: font_display: 'swap' }}

      .video-text-text-{{ block.id }} {
        text-align: {{ text_align_mobile }};
        margin-top: {{ block.settings.text_mt_mobile }}px;
      }

      .video-text-text-{{ block.id }} * {
        margin: 0;
        font-size: {{ block.settings.text_size_mobile }}px;
        color: {{ block.settings.text_color }};
        line-height: {{ block.settings.text_height }}%;
        text-transform: unset;
      }

      @media(min-width: 1024px) {

        .video-text-text-{{ block.id }} {
          text-align: {{ text_align }};
          margin-top: {{ block.settings.text_mt }}px;
        }

        .video-text-text-{{ block.id }} * {
          font-size: {{ block.settings.text_size }}px;
        }
      }
    </style>

    {% if block.settings.text_custom %}
      <style>
        .video-text-text-{{ block.id }} {
          font-family: {{ block.settings.text_font.family }}, {{ block.settings.text_font.fallback_families }};
          font-weight: {{ block.settings.text_font.weight }};
          font-style: {{ block.settings.text_font.style }};
        }
      </style>
    {% endif %}

    <style>
      .video-text-text-{{ block.id }} {
        order: 3;
      }

      @media(min-width: 1024px) {
        .video-text-text-{{ block.id }} {
          order: initial;
        }
      }
    </style>

  {% elsif block.type == 'button' %}
    <style>
      {{  block.settings.button_font | font_face: font_display: 'swap' }}

      .video-text-button-{{ block.id }} {
        display: flex;
        align-items: center;
        justify-content: center;
        gap: 10px;
        width: 100%;
        max-width: fit-content;
        margin: 0;
        font-size: {{ block.settings.button_size_mobile }}px;
        color: {{ block.settings.button_color }};
        line-height: {{ block.settings.button_height }}%;
        text-align: center;
        text-transform: unset;
        text-decoration: none;
        padding: {{ block.settings.button_padding_vertical_mobile }}px {{ block.settings.button_padding_horizontal_mobile }}px;
